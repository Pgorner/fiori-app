<mvc:View
    xmlns="sap.m"
    xmlns:tnt="sap.tnt"
    xmlns:core="sap.ui.core"
    xmlns:mvc="sap.ui.core.mvc"
    controllerName="sap.ushell.components.shell.SideNavigation.controller.SideNavigation"
    displayBlock="true"
    height="100%"
    core:require="{
        mLibrary: 'sap/m/library'
    }">
    <VBox
        class="sapUiSizeCompact"
        renderType="{= mLibrary.FlexRendertype.Bare }"
        alignItems="Start"
        height="100%">
        <tnt:SideNavigation
            id="sideNavigation"
            itemSelect=".onSideNavigationItemSelection"
            selectedKey="{viewConfiguration>/selectedKey}">
            <tnt:NavigationList
                selectedKey="{viewConfiguration>/selectedKey}"
                items="{
                    path: 'sideNavigation>/',
                    factory: '._sideNavigationFactory'
                }"/>
            <tnt:customData>
                <core:CustomData
                    key="sap-ui-fastnavgroup"
                    value="true"
                    writeToDom="true"/>
            </tnt:customData>
        </tnt:SideNavigation>
    </VBox>
</mvc:View>