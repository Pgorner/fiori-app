<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
    <Dialog
        id="pageNotExistsDialog"
        title="Error"
        class="sapUiResponsivePadding"
        state="Error">
        <content>
            <VBox class="sapUiSmallMargin">
                <Text text="{errorPage>/details}" />
                <Text text="{errorPage>/pageAndSpaceId}" />
            </VBox>
        </content>
        <beginButton>
            <Button
                id="pageNotExistsDialogCopyButton"
                text="Copy"
                press="._copyToClipboard" />
        </beginButton>
        <endButton>
            <Button
                id="pageNotExistsDialogCancelButton"
                text="Close"
                press="._onDialogClose"
                type="Emphasized" />
        </endButton>
    </Dialog>
</core:FragmentDefinition>
