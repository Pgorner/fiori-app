<core:FragmentDefinition
        xmlns="sap.m"
        xmlns:smartfilterbar="sap.ui.comp.smartfilterbar"
        xmlns:core="sap.ui.core">

    <smartfilterbar:ControlConfiguration key="CustomGrossAmountFilter" index="3" label="Gross Amount (Ext)"
                                         visibleInAdvancedArea="true" groupId="_BASIC">
        <smartfilterbar:customControl>
            <ComboBox id="CustomFilter-Price-combobox">
                <core:Item key="0" text="LT 10000"/>
                <core:Item key="1" text="GE 10000"/>
            </ComboBox>
        </smartfilterbar:customControl>
    </smartfilterbar:ControlConfiguration>
    <smartfilterbar:ControlConfiguration id="LastChangedDatewithTime" groupId="_BASIC" index="20" key="LastChangedDateTime" label="Changed At(Custom Control)" conditionType="{ module: 'sap.ui.comp.config.condition.DateRangeType', operations: { filter: { path: 'key', contains: ['YEAR','QUARTER','WEEK','MONTH'], exclude:true } } }">
    </smartfilterbar:ControlConfiguration>
    <smartfilterbar:ControlConfiguration id="DeliveryDateTime" timezone="Europe/Berlin" key="DeliveryDateTime" label="DeliveryDateTime Europe" >
    </smartfilterbar:ControlConfiguration>
</core:FragmentDefinition>