{
	"_version": "1.2.0",
	"sap.app": {
		"_version": "1.2.0",
		"id": "SOwoExt",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.132.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"tags": {
			"keywords": []
		},
		"ach": "CA-UI5-ST",
		"resources": "resources.json",
		"dataSources": {
			"mainService": {
				"uri": "/FolderName_sample.stta.sales.order.no.extensions/sap/opu/odata/sap/STTA_SALES_ORDER_WD_20_SRV/",
				"type": "OData",
				"settings": {
					"annotations": [
						"STTA_SALES_ORDER_WD_20_ANNO_MDL",
						"localAnnotations"
					],
					"localUri": "localService/metadata.xml"
				}
			},
			"STTA_SALES_ORDER_WD_20_ANNO_MDL": {
				"uri": "/FolderName_sample.stta.sales.order.no.extensions/sap/opu/odata/IWFND/CATALOGSERVICE;v=2/Annotations(TechnicalName='STTA_SALES_ORDER_WD_20_ANNO_MDL',Version='0001')/$value/",
				"type": "ODataAnnotation",
				"settings": {
					"localUri": "localService/STTA_SALES_ORDER_WD_20_ANNO_MDL.xml"
				}
			},
			"localAnnotations": {
				"uri": "/FolderName_sample.stta.sales.order.no.extensions/annotations/annotations.xml",
				"type": "ODataAnnotation",
				"settings": {
					"localUri": "annotations/annotations.xml"
				}
			}
		},
		"offline": false,
		"sourceTemplate": {
			"id": "ui5template.smarttemplate",
			"version": "1.40.12"
		},
		"crossNavigation": {
			"outbounds": {
				"BusinessPartnerNavigation": {
					"semanticObject": "BusinessPartner",
					"action": "displayFactSheet"
				},
				"ManageProductNavigation": {
					"semanticObject": "EPMManageProduct",
					"action": "displayFactSheet"
				}
			}
		}
	},
	"sap.ui": {
		"_version": "1.2.0",
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_bluecrystal",
			"sap_belize"
		]
	},
	"sap.ui5": {
		"_version": "1.1.0",
		"dependencies": {
			"minUI5Version": "${sap.ui5.dist.version}",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.comp": {},
				"sap.uxap": {},
				"sap.suite.ui.generic.template": {}
			},
			"components": {}
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"uri": "i18n/i18n.properties"
			},
			"@i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"uri": "i18n/i18n.properties"
			},
			"i18n|sap.suite.ui.generic.template.ListReport|C_STTA_SalesOrder_WD_20": {
				"type": "sap.ui.model.resource.ResourceModel",
				"uri": "i18n/ListReport/C_STTA_SalesOrder_WD_20/i18n.properties"
			},
			"i18n|sap.suite.ui.generic.template.ObjectPage|C_STTA_SalesOrder_WD_20": {
				"type": "sap.ui.model.resource.ResourceModel",
				"uri": "i18n/ObjectPage/C_STTA_SalesOrder_WD_20/i18n.properties"
			},
			"i18n|sap.suite.ui.generic.template.ObjectPage|C_STTA_SalesOrderItem_WD_20": {
				"type": "sap.ui.model.resource.ResourceModel",
				"uri": "i18n/ObjectPage/C_STTA_SalesOrderItem_WD_20/i18n.properties"
			},
			"": {
				"dataSource": "mainService",
				"settings": {
					"defaultBindingMode": "TwoWay",
					"defaultCountMode": "Inline",
					"refreshAfterChange": false
				}
			}
		},
		"extends": {
			"extensions":  {
				"sap.ui.commands": {
					"sap.suite.ui.generic.template.ObjectPage#sap.suite.ui.generic.template.ObjectPage::C_STTA_SalesOrder_WD_20": {
						"OppItemCommand": {
							"shortcut": "Ctrl+B"
						
						}
					}
				},
				"sap.ui.controllerExtensions": {
					"sap.suite.ui.generic.template.ListReport.view.ListReport": {
						"controllerName": "SOwoExt.ext.controller.ListReportExtension"
					},
					"sap.suite.ui.generic.template.ObjectPage.view.Details": {
						"controllerName": "SOwoExt.ext.controller.ObjectPageExtension"
					}
				},
				"sap.ui.viewExtensions": {
					"sap.suite.ui.generic.template.ListReport.view.ListReport": {
						"ResponsiveTableColumnsExtension|C_STTA_SalesOrder_WD_20": {
                            "className": "sap.ui.core.Fragment",
                            "fragmentName": "SOwoExt.ext.fragments.ListReportResponsiveTableColumns",
                            "type": "XML"
                        },
                        "ResponsiveTableCellsExtension|C_STTA_SalesOrder_WD_20": {
                            "className": "sap.ui.core.Fragment",
                            "fragmentName": "SOwoExt.ext.fragments.ListReportResponsiveTableCells",
                            "type": "XML"
                        }
					},
					"sap.suite.ui.generic.template.ObjectPage.view.Details": {
						"ResponsiveTableColumnsExtension|C_STTA_SalesOrderItem_WD_20|to_Item::com.sap.vocabularies.UI.v1.LineItem": {
                            "className": "sap.ui.core.Fragment",
                            "fragmentName": "SOwoExt.ext.fragments.SalesOrderItemResponsiveTableColumns",
                            "type": "XML"
                        },
                        "ResponsiveTableCellsExtension|C_STTA_SalesOrderItem_WD_20|to_Item::com.sap.vocabularies.UI.v1.LineItem": {
                            "className": "sap.ui.core.Fragment",
                            "fragmentName": "SOwoExt.ext.fragments.SalesOrderItemResponsiveTableCells",
                            "type": "XML"
                        },
						"SmartFormExtension|C_STTA_SalesOrder_WD_20|com.sap.vocabularies.UI.v1.FieldGroup::GeneralInformation": {
							"className": "sap.ui.core.Fragment",
							"fragmentName": "SOwoExt.ext.fragments.GeneralInfoFormExt",
							"type": "XML"
						}
					}
				}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		}
	},
	"sap.ui.generic.app": {
		"_version": "1.1.0",
	 	"settings": {
			"showDraftToggle": false,
			"useColumnLayoutForSmartForm" : true,
			"statePreservationMode": "persistence"
	 	},
		"pages": [
			{
				"entitySet": "C_STTA_SalesOrder_WD_20",
				"component": {
					"name": "sap.suite.ui.generic.template.ListReport",
					"list": true,
					"settings": {
						"dataLoadSettings" : {
							"loadDataOnAppLaunch" : "always"
						}
					}
				},
				"pages": [
					{
						"entitySet": "C_STTA_SalesOrder_WD_20",
						"component": {
							"name": "sap.suite.ui.generic.template.ObjectPage",
							"settings": {
								"sections": {
									"to_Item::com.sap.vocabularies.UI.v1.LineItem": {
										"navigationProperty": "to_Item",
										"entitySet": "C_STTA_SalesOrderItem_WD_20",
										"createMode": "inline"
									}
								},
								"annotatedActions": {
									"C_STTA_SalesOrderItem_WD_20Setopportunityid": {
										"command": "OppItemCommand"
									}
								}
							}
						},
						"pages": [
							{
								"navigationProperty": "to_Item",
								"entitySet": "C_STTA_SalesOrderItem_WD_20",
								"component": {
									"name": "sap.suite.ui.generic.template.ObjectPage"
								},
								"pages": [
									{
										"navigationProperty": "to_SalesOrderItemSL",
										"entitySet": "C_STTA_SalesOrderItemSL_WD_20",
										"component": {
											"name": "sap.suite.ui.generic.template.ObjectPage"
										}
									}
								]
							}
						]
					}
				]
			}
		]
	},
	"sap.fiori": {
		"_version": "1.1.0",
		"registrationIds": [],
		"archeType": "transactional"
	},
	"sap.platform.hcp": {
		"_version": "1.2.0",
		"uri": ""
	}
}
