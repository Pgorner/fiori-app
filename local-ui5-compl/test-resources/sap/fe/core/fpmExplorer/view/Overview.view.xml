<mvc:View controllerName="sap.fe.core.fpmExplorer.controller.Overview" xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m">
	<Page class="topicPage" showHeader="true" title="{/pageTitle}">
		<content>
			<core:HTML
				id="topicFrame"
				preferDOM="false"
				content='&lt;div class="sapUiTopicsDiv"&gt;&lt;iframe {/iframeAttribute} class="sapUiTopicsIframe" name="content" src="{/topicURL}" frameborder="0"&gt;&lt;/iframe&gt;&lt;/div&gt;'
			/>
		</content>
	</Page>
</mvc:View>
