<mvc:View
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m"
	xmlns:f="sap.f"
	xmlns:macros="sap.fe.macros"
	controllerName="sap.fe.core.fpmExplorer.FclNavigationButtons.myViewTest"
>
	<f:DynamicPage id="dynamicPageId" headerExpanded="true" toggleHeaderOnTitleClick="false">
		<!-- DynamicPage Title -->
		<f:title>
			<f:DynamicPageTitle id="incidentProcessFlowDynamicPageTitle">
				<f:heading>
					<Title id="incidentProcessFlowHeading" text="This is a title" />
				</f:heading>
				<f:navigationActions>
					<macros:FlexibleColumnLayoutActions />
				</f:navigationActions>
			</f:DynamicPageTitle>
		</f:title>
		<f:content>
			<MessageStrip
				text="This example needs to be expanded to see the feature. Please expand this window to see the different columns."
				visible="true"
				showIcon="true"
				type="Warning"
				class="sapUiSmallMargin"
			/>
		</f:content>
	</f:DynamicPage>
</mvc:View>
