<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m">
	<ComboBox
		width="100%"
		selectedKey="{path: 'filterValues>', type: 'sap.fe.macros.filter.type.Value', formatOptions: { operator: 'sap.fe.core.fpmExplorer.filterBarCustoms.CustomFilter.ratingOperator' }}"
	>
		<items>
			<core:Item key="Empty" />
			<core:Item key="Low" text="Low" />
			<core:Item key="Medium" text="Medium" />
			<core:Item key="High" text="High" />
		</items>
	</ComboBox>
</core:FragmentDefinition>
