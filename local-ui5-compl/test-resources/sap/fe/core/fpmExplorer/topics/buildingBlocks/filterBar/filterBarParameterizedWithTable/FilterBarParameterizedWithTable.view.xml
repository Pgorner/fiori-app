<mvc:View
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:core="sap.ui.core"
	xmlns:macros="sap.fe.macros"
	xmlns:ce="sap.ui.codeeditor"
	xmlns:l="sap.ui.layout"
	xmlns:v="sap.ui.fl.variants"
	xmlns:f="sap.f"
	controllerName="sap.fe.core.fpmExplorer.filterBarParameterizedWithTable.FilterBarParameterizedWithTable"
>
	<f:DynamicPage id="FilterBarParameterizedWithTable" class="sapUiResponsiveContentPadding">
		<f:header>
			<f:DynamicPageHeader id="_IDGenDynamicPageHeader1" pinnable="true">
				<VBox id="_IDGenVBox1">
					<macros:FilterBar
						metaPath="@com.sap.vocabularies.UI.v1.SelectionFields"
						id="FilterBar"
						showMessages="{fbConditions>/showMessageDialog}"
					/>
				</VBox>
			</f:DynamicPageHeader>
		</f:header>
		<f:content>
			<macros:Table metaPath="@com.sap.vocabularies.UI.v1.LineItem" readOnly="true" id="LineItemTable" filterBar="FilterBar" />
		</f:content>
	</f:DynamicPage>
</mvc:View>
