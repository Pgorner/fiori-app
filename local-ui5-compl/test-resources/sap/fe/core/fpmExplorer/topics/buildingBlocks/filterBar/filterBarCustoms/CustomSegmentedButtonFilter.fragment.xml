<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m">
	<SegmentedButton
		id="CustomSegmentedButton"
		core:require="{handler: 'sap/fe/core/fpmExplorer/filterBarCustoms/CustomFilter'}"
		selectionChange="handler.onRatingChoicesSelectionChange"
	>
		<items>
			<SegmentedButtonItem key="none" text="None" />
			<SegmentedButtonItem key="bad" text="Bad" />
			<SegmentedButtonItem key="okay" text="Okay" />
			<SegmentedButtonItem key="good" text="Good" />
		</items>
	</SegmentedButton>
</core:FragmentDefinition>
