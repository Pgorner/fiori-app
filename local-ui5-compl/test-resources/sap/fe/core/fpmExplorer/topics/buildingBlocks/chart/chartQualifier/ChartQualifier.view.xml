<mvc:View
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:macros="sap.fe.macros"
	controllerName="sap.fe.core.fpmExplorer.chartQualifier.ChartQualifier"
>
	<VBox fitContainer="true" alignItems="Stretch" alignContent="Stretch" justifyContent="Start" renderType="Div">
		<macros:Chart
			id="chartQualifier"
			metaPath="/RootEntity/@com.sap.vocabularies.UI.v1.Chart#ChartQualifier"
			selectionChange=".onChartSelectionChanged"
			personalization="Type,Item"
			selectionMode="Multiple"
		/>
	</VBox>
</mvc:View>
