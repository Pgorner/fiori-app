{
	"_version": "1.12.0",
	"sap.app": {
		"id": "sap.fe.core.fpmExplorer.guidanceCustomApps",
		"type": "application",
		"i18n": {
			"bundleUrl": "i18n/i18n.properties",
			"supportedLocales": [""],
			"fallbackLocale": ""
		},
		"applicationVersion": {
			"version": "1.0.0"
		},
		"dataSources": {
			"mainService": {
				"uri": "/sap/fe/core/mock/guidanceCustomApps/",
				"type": "OData",
				"settings": {
					"annotations": [],
					"odataVersion": "4.0"
				}
			}
		}
	},
	"sap.ui5": {
		"dependencies": {
			"minUI5Version": "1.111.1",
			"libs": {
				"sap.ui.core": {},
				"sap.fe.macros": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleUrl": "i18n/i18n.properties"
				}
			},
			"@i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleUrl": "i18n/i18n.properties"
				}
			},
			"": {
				"dataSource": "mainService",
				"settings": {
					"operationMode": "Server",
					"autoExpandSelect": true,
					"earlyRequests": true
				}
			}
		},

		"routing": {
			"routes": [
				{
					"pattern": ":?query:",
					"name": "entryPage",
					"target": "entryPage"
				},
				{
					"pattern": "/RootEntity({key}):?query:",
					"name": "detailPage",
					"target": "detailPage"
				}
			],
			"targets": {
				"entryPage": {
					"type": "Component",
					"id": "entryPage",
					"name": "sap.fe.core.fpm",
					"options": {
						"settings": {
							"viewName": "sap/fe/core/fpmExplorer/guidanceCustomApps/EntryPage",
							"controllerName": "sap.fe.core.fpmExplorer.guidanceCustomApps.EntryPage",
							"viewType": "JSX",
							"contextPath": "/RootEntity",
							"navigation": {
								"RootEntity": {
									"detail": {
										"route": "detailPage"
									}
								}
							}
						}
					}
				},
				"detailPage": {
					"type": "Component",
					"id": "detailPage",
					"name": "sap.fe.core.fpm",
					"options": {
						"settings": {
							"viewName": "sap.fe.core.fpmExplorer.guidanceCustomApps.DetailPage",
							"contextPath": "/RootEntity",
							"navigation": {}
						}
					}
				}
			}
		}
	}
}
