<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m">
	<HBox alignItems="Center" core:require="{handler: 'sap/fe/core/fpmExplorer/filterBarCustoms/CustomFilter'}" width="100%">
		<RangeSlider width="100%" min="0" max="5" range="{path: 'filterValues>', type: 'sap.fe.macros.filter.type.Range' }">
			<layoutData>
				<FlexItemData growFactor="1" />
			</layoutData>
		</RangeSlider>
		<core:Icon src="sap-icon://reset" press="handler.onRatingRangeReset" class="sapUiSmallMarginBegin" />
	</HBox>
</core:FragmentDefinition>
