<core:FragmentDefinition
	xmlns:core="sap.ui.core"
	xmlns="sap.m"
	xmlns:l="sap.ui.layout"
	xmlns:form="sap.ui.layout.form"
	xmlns:macros="sap.fe.macros"
>
	<l:Grid hSpacing="0" vSpacing="0" containerQuery='true' defaultSpan='L6 M12 S12'>
		<l:content>
			<form:Form>
				<form:toolbar>
					<Toolbar id="TB1">
						<Title id="Title1" text="Form Element" />
						<ToolbarSpacer />
						<Button icon="sap-icon://settings" />
						<Button icon="sap-icon://drop-down-list" />
					</Toolbar>
				</form:toolbar>
				<form:layout>
					<form:ColumnLayout columnsM="2" columnsL="3" columnsXL="4" labelCellsLarge="12" />
				</form:layout>
				<form:formContainers>
					<form:FormContainer>
						<form:formElements>
							<macros:FormElement metaPath="TitleProperty" id="myFormElement1" />
							<macros:FormElement metaPath="NameProperty" id="myFormElement2" />
							<macros:FormElement metaPath="DescriptionProperty" id="myFormElement3" />
						</form:formElements>
					</form:FormContainer>
				</form:formContainers>
			</form:Form>
			<macros:Table metaPath="items/@com.sap.vocabularies.UI.v1.LineItem" id="LineItemTablePageEdit2" />
		</l:content>
	</l:Grid>
</core:FragmentDefinition>
