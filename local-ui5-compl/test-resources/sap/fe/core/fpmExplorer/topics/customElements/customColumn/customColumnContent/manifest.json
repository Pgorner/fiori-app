{
	"_version": "1.12.0",
	"sap.app": {
		"id": "sap.fe.core.fpmExplorer.customColumnContent",
		"type": "application",
		"i18n": {
			"bundleName": "sap.fe.core.fpmExplorer.customColumnContent.i18n.i18n",
			"supportedLocales": [""],
			"fallbackLocale": ""
		},
		"applicationVersion": {
			"version": "1.0.0"
		},
		"dataSources": {
			"mainService": {
				"uri": "/sap/fe/core/mock/customColumnContent/",
				"type": "OData",
				"settings": {
					"annotations": [],
					"odataVersion": "4.0"
				}
			}
		}
	},
	"sap.ui5": {
		"dependencies": {
			"minUI5Version": "1.111.1",
			"libs": {
				"sap.ui.core": {},
				"sap.fe.templates": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "sap.fe.core.fpmExplorer.customColumnContent.i18n.i18n"
				}
			},
			"@i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "sap.fe.core.fpmExplorer.customColumnContent.i18n.i18n"
				}
			},
			"": {
				"dataSource": "mainService",
				"settings": {
					"operationMode": "Server",
					"autoExpandSelect": true,
					"earlyRequests": true
				}
			}
		},
		"routing": {
			"routes": [
				{
					"pattern": "/RootEntity({key}):?query:",
					"name": "default",
					"target": "sample"
				}
			],
			"targets": {
				"sample": {
					"type": "Component",
					"id": "Default",
					"name": "sap.fe.templates.ObjectPage",
					"viewLevel": 1,
					"options": {
						"settings": {
							"contextPath": "/RootEntity",
							"controlConfiguration": {
								"items/@com.sap.vocabularies.UI.v1.LineItem": {
									"tableSettings": {
										"selectionMode": "Single"
									},
									"columns": {
										"CustomColumnOnObjectPage": {
											"header": "{i18n>customColumn}",
											"width": "15em",
											"importance": "High",
											"horizontalAlign": "Center",
											"position": {
												"placement": "After",
												"anchor": "DataField::BooleanProperty"
											},
											"template": "sap.fe.core.fpmExplorer.customColumnContent.CustomColumnButton",
											"availability": "Default",
											"properties": ["BooleanProperty", "TextProperty"]
										},
										"anotherCustomColumnOnObjectPage": {
											"header": "{i18n>customColumnAdaptation}",
											"width": "15em",
											"importance": "Medium",
											"horizontalAlign": "Begin",
											"template": "sap.fe.core.fpmExplorer.customColumnContent.CustomColumnAdaptation",
											"availability": "Adaptation"
										},
										"CustomColumnWithFieldBuildingBlockOnObjectPage": {
											"header": "{i18n>customColumnField}",
											"width": "15em",
											"importance": "Low",
											"horizontalAlign": "End",
											"template": "sap.fe.core.fpmExplorer.customColumnContent.CustomColumnBuildingBlockField",
											"availability": "Default"
										},
										"CustomColumnWithHeaderViaMetaDataBindingPathOnObjectPage": {
											"header": "{metaModel>/ChildEntity/TextProperty@com.sap.vocabularies.Common.v1.Label}",
											"width": "15em",
											"importance": "Low",
											"horizontalAlign": "End",
											"template": "sap.fe.core.fpmExplorer.customColumnContent.CustomColumnBuildingBlockField",
											"availability": "Default"
										}
									}
								}
							}
						}
					}
				}
			}
		}
	}
}
