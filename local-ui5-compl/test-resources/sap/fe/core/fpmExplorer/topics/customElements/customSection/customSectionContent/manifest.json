{
	"_version": "1.12.0",
	"sap.app": {
		"id": "sap.fe.core.fpmExplorer.customSectionContent",
		"type": "application",
		"i18n": {
			"bundleName": "sap.fe.core.fpmExplorer.customSectionContent.i18n.i18n",
			"supportedLocales": [""],
			"fallbackLocale": ""
		},
		"applicationVersion": {
			"version": "1.0.0"
		},
		"dataSources": {
			"mainService": {
				"uri": "/sap/fe/core/mock/customSectionContent/",
				"type": "OData",
				"settings": {
					"annotations": [],
					"odataVersion": "4.0"
				}
			}
		}
	},
	"sap.ui5": {
		"dependencies": {
			"minUI5Version": "1.111.1",
			"libs": {
				"sap.ui.core": {},
				"sap.fe.templates": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "sap.fe.core.fpmExplorer.customSectionContent.i18n.i18n"
				}
			},
			"@i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "sap.fe.core.fpmExplorer.customSectionContent.i18n.i18n"
				}
			},
			"": {
				"dataSource": "mainService",
				"settings": {
					"operationMode": "Server",
					"autoExpandSelect": true,
					"earlyRequests": true
				}
			}
		},
		"extends": {
			"extensions": {
				"sap.ui.controllerExtensions": {
					"sap.fe.templates.ObjectPage.ObjectPageController": {
						"controllerName": "sap.fe.core.fpmExplorer.customSectionContent.extendOP"
					}
				}
			}
		},
		"routing": {
			"routes": [
				{
					"pattern": "/RootEntity({key}):?query:",
					"name": "default",
					"target": "sample"
				},
				{
					"pattern": "/RootEntity({key})/items({key2}):?query:",
					"name": "item",
					"target": "item"
				}
			],
			"targets": {
				"item": {
					"type": "Component",
					"id": "item",
					"name": "sap.fe.templates.ObjectPage",
					"options": {
						"settings": {
							"contextPath": "/RootEntity/items"
						}
					}
				},
				"sample": {
					"type": "Component",
					"id": "Default",
					"name": "sap.fe.templates.ObjectPage",
					"viewLevel": 1,
					"options": {
						"settings": {
							"contextPath": "/RootEntity",
							"navigation": {
								"items": {
									"detail": {
										"route": "item"
									}
								}
							},
							"content": {
								"body": {
									"sections": {
										"myCustomSection": {
											"template": "sap.fe.core.fpmExplorer.customSectionContent.CustomSection",
											"title": "{i18n>MyCustomSection}",
											"position": {
												"placement": "Before",
												"anchor": "StandardSection"
											}
										},
										"customSectionRelativePositioning": {
											"template": "sap.fe.core.fpmExplorer.customSectionContent.CustomSectionFormElementAndTable",
											"title": "{i18n>MyFormElementAndMacroTable}",
											"position": {
												"placement": "After",
												"anchor": "StandardSection"
											}
										},
										"customSectionRelativePositioning_2": {
											"template": "sap.fe.core.fpmExplorer.customSectionContent.CustomSectionNavigation",
											"title": "{i18n>MyNavigation}",
											"visible": "{= !${ui>/isEditable}}",
											"position": {
												"placement": "After",
												"anchor": "customSectionRelativePositioning"
											}
										}
									}
								}
							}
						}
					}
				}
			}
		}
	}
}
