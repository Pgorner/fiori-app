<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m">
	<HBox alignItems="Center" core:require="{handler: 'sap/fe/core/fpmExplorer/customFilterContent/CustomFilter'}" width="100%">
		<MultiComboBox
			selectedKeys="{path: 'filterValues>', type: 'sap.fe.macros.filter.type.MultiValue', formatOptions: { operator: 'sap.fe.core.fpmExplorer.customFilterContent.CustomFilter.multiValueRatingLevels' }}"
		>
			<layoutData>
				<FlexItemData growFactor="1" />
			</layoutData>
			<items>
				<core:Item key="low" text="low" />
				<core:Item key="medium" text="medium" />
				<core:Item key="high" text="high" />
			</items>
		</MultiComboBox>
	</HBox>
</core:FragmentDefinition>
