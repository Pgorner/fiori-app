{
	"_version": "1.12.0",
	"sap.app": {
		"id": "sap.fe.core.fpmExplorer",
		"type": "application",
		"i18n": {
			"bundleUrl": "i18n/i18n.properties",
			"supportedLocales": [""],
			"fallbackLocale": ""
		},
		"applicationVersion": {
			"version": "1.0.0"
		},
		"dataSources": {
			"mainService": {
				"uri": "/sap/fe/core/mock/service/",
				"type": "OData",
				"settings": {
					"annotations": [],
					"odataVersion": "4.0"
				}
			}
		}
	},
	"sap.ui5": {
		"dependencies": {
			"minUI5Version": "1.111.1",
			"libs": {
				"sap.ui.core": {},
				"sap.fe.templates": {}
			}
		},
		"extends": {
			"extensions": {
				"sap.ui.controllerExtensions": {
					"sap.fe.templates.ListReport.ListReportController": {
						"controllerName": "sap.fe.core.fpmExplorer.extendLR"
					}
				}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"": {
				"dataSource": "mainService",
				"settings": {
					"operationMode": "Server",
					"autoExpandSelect": true,
					"earlyRequests": true
				}
			}
		},
		"routing": {
			"routes": [
				{
					"pattern": ":?query:",
					"name": "root",
					"target": "listReport"
				}
			],
			"targets": {
				"listReport": {
					"type": "Component",
					"id": "sample",
					"name": "sap.fe.templates.ListReport",
					"options": {
						"settings": {
							"contextPath": "/RootEntity",
							"variantManagement": "None",
							"initialLoad": "Enabled",
							"controlConfiguration": {
								"@com.sap.vocabularies.UI.v1.LineItem#Rating": {
									"actions": {
										"ratingUpdate": {
											"press": ".extension.sap.fe.core.fpmExplorer.extendLR.onPressRatingUpdate",
											"text": "Update Rating",
											"requiresSelection": true,
											"enabled": true
										}
									}
								},
								"@com.sap.vocabularies.UI.v1.LineItem#Status": {
									"actions": {
										"statusUpdate": {
											"press": ".extension.sap.fe.core.fpmExplorer.extendLR.onPressStatusUpdate",
											"text": "Update Status",
											"requiresSelection": true,
											"enabled": true
										}
									}
								}
							},
							"views": {
								"paths": [
									{
										"key": "tab1",
										"annotationPath": "com.sap.vocabularies.UI.v1.SelectionVariant#All"
									},
									{
										"key": "tab2",
										"annotationPath": "com.sap.vocabularies.UI.v1.SelectionPresentationVariant#GoodRating"
									},
									{
										"key": "tab3",
										"annotationPath": "com.sap.vocabularies.UI.v1.SelectionPresentationVariant#BadRating"
									},
									{
										"key": "tab4",
										"annotationPath": "com.sap.vocabularies.UI.v1.SelectionPresentationVariant#OpenStatus"
									},
									{
										"key": "tab5",
										"annotationPath": "com.sap.vocabularies.UI.v1.SelectionPresentationVariant#CompletedStatus"
									},
									{
										"key": "tab6",
										"label": "Custom View with Table Macro",
										"template": "sap.fe.core.fpmExplorer.CustomViewWithTableMacro"
									}
								],
								"showCounts": true
							}
						}
					}
				}
			}
		}
	}
}
