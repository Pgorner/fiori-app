<mvc:View controllerName="sap.fe.core.fpmExplorer.IntentBasedNavigation" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout">
	<l:VerticalLayout class="sapUiContentPadding">
		<Text text="Parameters provided here will be passed to the external intent" />
		<TextArea
			rows="4"
			placeholder='\{"SalesOrder":"234567", "Category": "OR"\}'
			value='\{"SalesOrder":"234567", "Category": "OR"\}'
			id="navParameters"
		/>
		<Button text="Navigate to Intent" press=".navigateExternal" />
	</l:VerticalLayout>
</mvc:View>
