{
	"_version": "1.12.0",
	"sap.app": {
		"id": "sap.fe.core.fpmExplorer",
		"type": "application",
		"i18n": {
			"bundleUrl": "i18n/i18n.properties",
			"supportedLocales": [""],
			"fallbackLocale": ""
		},
		"applicationVersion": {
			"version": "1.0.0"
		},
		"dataSources": {
			"mainService": {
				"uri": "/sap/fe/core/mock/service/",
				"type": "OData",
				"settings": {
					"annotations": [],
					"odataVersion": "4.0"
				}
			}
		}
	},
	"sap.ui5": {
		"dependencies": {
			"minUI5Version": "1.111.1",
			"libs": {
				"sap.ui.core": {},
				"sap.fe.templates": {}
			}
		},
		"extends": {
			"extensions": {
				"sap.ui.controllerExtensions": {
					"sap.fe.templates.ObjectPage.ObjectPageController#sap.fe.core.fpmExplorer::Default": {
						"controllerName": "sap.fe.core.fpmExplorer.OPExtend"
					},
					"sap.fe.templates.ObjectPage.ObjectPageController#sap.fe.core.fpmExplorer::DefaultSubOP": {
						"controllerName": "sap.fe.core.fpmExplorer.SubOPExtend"
					}
				}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"": {
				"dataSource": "mainService",
				"settings": {
					"operationMode": "Server",
					"autoExpandSelect": true,
					"earlyRequests": true
				}
			}
		},
		"routing": {
			"routes": [
				{
					"pattern": "/RootEntity({key}):?query:",
					"name": "default",
					"target": "sample"
				},
				{
					"pattern": "/RootEntity({key})/_Child({key2}):?query:",
					"name": "defaultSubOP",
					"target": "sampleSubOP"
				}
			],
			"targets": {
				"sample": {
					"type": "Component",
					"id": "Default",
					"name": "sap.fe.templates.ObjectPage",
					"viewLevel": 1,
					"options": {
						"settings": {
							"contextPath": "/RootEntity",
							"content": {
								"header": {
									"actions": {
										"headerCreateAction": {
											"press": ".extension.sap.fe.core.fpmExplorer.OPExtend.onCreatePress",
											"text": "Create",
											"position": {
												"placement": "Before",
												"anchor": "headerBoundAction"
											}
										},
										"headerBoundAction": {
											"press": ".extension.sap.fe.core.fpmExplorer.OPExtend.onBoundPress",
											"text": "Bound Action"
										},
										"headerUnBoundAction": {
											"press": ".extension.sap.fe.core.fpmExplorer.OPExtend.onUnboundPress",
											"text": "Unbound Action",
											"enabled": "{= ${BooleanProperty}}",
											"position": {
												"placement": "After",
												"anchor": "headerBoundAction"
											}
										},
										"headerBoundActionWithParameters": {
											"press": ".extension.sap.fe.core.fpmExplorer.OPExtend.onBoundPressActionWithParameters",
											"text": "Action With Parameter Values",
											"position": {
												"placement": "After",
												"anchor": "headerUnBoundAction"
											}
										},
										"headerBoundActionWithParametersSkipDialog": {
											"press": ".extension.sap.fe.core.fpmExplorer.OPExtend.onBoundPressActionWithParametersSkipDialog",
											"text": "Action With Parameter Values Skip Dialog",
											"position": {
												"placement": "After",
												"anchor": "headerBoundActionWithParameters"
											}
										}
									}
								}
							},
							"controlConfiguration": {
								"_Child/@com.sap.vocabularies.UI.v1.LineItem": {
									"actions": {
										"customActionSetTitle": {
											"press": ".extension.sap.fe.core.fpmExplorer.OPExtend.onBoundSetTitlePress",
											"text": "Set Title",
											"requiresSelection": true
										}
									}
								}
							},
							"navigation": {
								"ChildEntity": {
									"detail": {
										"route": "sampleSubOP"
									}
								}
							}
						}
					}
				},
				"sampleSubOP": {
					"type": "Component",
					"id": "DefaultSubOP",
					"name": "sap.fe.templates.ObjectPage",
					"viewLevel": 2,
					"options": {
						"settings": {
							"contextPath": "/RootEntity/_Child"
						}
					}
				}
			}
		}
	}
}
