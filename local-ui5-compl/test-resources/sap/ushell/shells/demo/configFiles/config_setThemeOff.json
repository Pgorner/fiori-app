{
  "services": {
    "NavTargetResolution": {
      "config": {
        "allowTestUrlComponentConfig": true
      }
    },
    "SupportTicket": {
      "config": {
        "enabled": true
      }
    },
    "UserInfo": {
      "adapter": {
        "config": {
          "themes": [
            {
              "id": "sap_horizon",
              "name": "SAP Morning Horizon"
            }
          ]
        }
      }
    }
  },
  "renderers": {
    "fiori2": {
      "componentData": {
        "config": {
          "enableSetTheme": false,
          "applications": {
            "Shell-home": {
              "enableActionModeMenuButton": true,
              "enableActionModeFloatingButton": true,
              "enableTileActionsIcon": true,
              "enableHideGroups": true
            }
          },
          "rootIntent": "Shell-home"
        }
      }
    }
  }
}
