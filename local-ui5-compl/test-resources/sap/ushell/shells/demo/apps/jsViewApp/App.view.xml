<mvc:View
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:mvc="sap.ui.core.mvc"
    controllerName="shells.demo.apps.jsViewApp.App">
    <App>
        <Page title="letterBoxing Application">
            <content>
                <Panel headerText="letterBoxing application">
                    <List id="letterBoxing" noDataText="">
                        <CustomListItem>
                            <Button
                                id="idChangeLetterBoxButton"
                                class="letterboxingBtn idQunitChangeLetterBoxButton"
                                text="Change letterBoxing"
                                press="onChangeLetterBoxing"
                                icon="sap-icon://change" />
                        </CustomListItem>
                    </List>
                </Panel>
            </content>
        </Page>
    </App>
</mvc:View>
