{
   "sap.flp": {
       "columns": "4",
       "rows": "3"
   },
   "sap.app": {
       "type": "card"
   },
   "sap.card": {
       "extension": "./Extension",
       "data": {
           "extension": {
               "method": "getData"
           }
       },
       "type": "List",
       "header": {
           "title": "Frequently Used",
           "status": {
               "text": "Top 3"
           },
           "actions": [
                {
                    "type": "Navigation",
                    "parameters": {
                        "openUI": "FrequentActivities"
                    }
                }
            ]
       },
       "content": {
           "maxItems": 3,
           "item": {
               "title": {
                   "value": "{Name}"
               },
               "description": {
                   "value": "{Description}"
               },
               "highlight": "{Highlight}",
               "icon": {
                    "src": "sap-icon://product",
                    "label": "icon"
               },
               "actions": [
                    {
                        "type": "Navigation",
                        "enabled": "{Enabled}",
                        "parameters": {
                            "title": "{Name}",
                            "url": "{Url}"
                        }
                    }
               ]
           }
       }
   }
}
