<mvc:View xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
    controllerName="sap.ushell.playground.controller.NavigationList">
    <Page title="Controls">
        <List items="{pages>/}">
            <items>
                <StandardListItem title="{pages>title}" type="Navigation" press=".navigateTo(${pages>route})" />
            </items>
        </List>
    </Page>
</mvc:View>
