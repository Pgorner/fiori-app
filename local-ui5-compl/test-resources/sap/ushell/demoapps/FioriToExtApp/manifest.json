{
    "_version": "1.21.0",

    "sap.fiori": {
        "registrationIds": ["F6407"]
    },

    "sap.app": {
        "_version": "1.1.0",
        "id": "sap.ushell.demo.FioriToExtApp",
        "type": "application",
        "i18n": {
            "bundleUrl": "i18n/i18n.properties",
            "supportedLocales": [
                "",
                "de"
            ],
            "fallbackLocale": ""
        },
        "title": "Fiori Source App",
        "subTitle": "{{subtitle}}",
        "description": "{{description}}",
        "applicationVersion": {
            "version": "1.1.0"
        },
        "ach": "CA-FLP-FE-UI",
        "dataSources": {},
        "cdsViews": [],
        "offline": true,
        "crossNavigation": {
            "inbounds": {
                "inb" :{
                    "semanticObject": "Action",
                    "action": "FioriToExtApp",
                    "signature": {
                        "parameters": {},
                        "additionalParameters": "allowed"
                    }
                }
            }
        }
    },
    "sap.ui": {
        "_version": "1.1.0",

        "technology": "UI5",
        "icons": {
            "icon": "sap-icon://Fiori2/F0429"
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        },
        "supportedThemes": [
            "sap_hcb",
            "sap_bluecrystal"
        ],
        "fullWidth": true
    },
    "sap.ui5": {
        "_version": "1.1.0",
        "resources": {
            "js": [],
            "css": [{
                "uri": "css/custom.css",
                "id": "sap.ushell.demo.FioriToExtApp.stylesheet"
            }]
        },
        "dependencies": {
            "minUI5Version":"1.28",
            "libs": {
                "sap.m": {
                    "minVersion": "1.28"
                }
            }
        },
        "models": {},
        "rootView": {
            "viewName": "sap.ushell.demo.FioriToExtApp.App",
            "type": "XML",
            "async": true,
            "id": "MainView"
        },
        "handleValidation": false,
        "config": {},
        "routing": {},
        "contentDensities": {
            "compact": false,
            "cozy": true
        },
        "services": {
            "ShellUIService": {
                "factoryName": "sap.ushell.ui5service.ShellUIService"
            },
            "CrossApplicationNavigation": {
                "factoryName": "sap.ushell.ui5service.CrossApplicationNavigation"
            },
            "URLParsing": {
                "factoryName": "sap.ushell.ui5service.URLParsing"
            },
            "Configuration": {
                "factoryName": "sap.ushell.ui5service.Configuration"
            }
        }
    }
}
