<mvc:View
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:mvc="sap.ui.core.mvc"
    controllerName="sap.ushell.demo.NavigationSample.controller.InitialLoadHeader">

    <Toolbar design="Transparent">
        <Title text="{i18n>initialNavigation.title}" />
        <core:Icon
            id="isInitialNavigationIcon"
            src="{initialLoad>/isInitialNavigationIcon}"
            color="{initialLoad>/isInitialNavigationColor}" />
        <Text id="isInitialNavigation" text="{initialLoad>/isInitialNavigation}" />
        <ToolbarSpacer />
    </Toolbar>

</mvc:View>
