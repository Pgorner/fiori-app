{
  "_version": "1.21.0",
  "sap.app": {
    "_version": "1.1.0",
    "id": "sap.ushell.demo.Fiori2AdaptationSample",
    "type": "application",
    "i18n": {
      "bundleUrl": "i18n/i18n.properties",
      "supportedLocales": [
        ""
      ],
      "fallbackLocale": ""
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "applicationVersion": {
      "version": "1.0.0"
    }
  },
  "sap.ui": {
    "_version": "1.1.0",
    "technology": "UI5",
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "_version": "1.1.0",
    "rootView": {
      "viewName": "sap.ushell.demo.Fiori2AdaptationSample.view.App",
      "type": "XML",
      "async": true
    },
    "dependencies": {
      "minUI5Version": "1.30",
      "libs": {
        "sap.m": {}
      }
    },
    "config": {
      "sapFiori2Adaptation": {
        "style": true,
        "collapse": true,
        "title": true,
        "back": true,
        "hierarchy": true
      }
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "sap.ushell.demo.Fiori2AdaptationSample.i18n.i18n",
          "supportedLocales": [
            ""
          ],
          "fallbackLocale": ""
        }
      }
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "sap.ushell.demo.Fiori2AdaptationSample.view",
        "controlId": "app",
        "controlAggregation": "pages"
      },
      "routes": [
        {
          "pattern": "",
          "name": "app",
          "target": "app"
        },
        {
          "pattern": "page2",
          "name": "page2",
          "target": "page2"
        },
        {
          "pattern": "page3",
          "name": "page3",
          "target": "page3"
        },
        {
          "pattern": "page4",
          "name": "page4",
          "target": "page4"
        }
      ],
      "targets": {
        "app": {
          "viewName": "App"
        },
        "page2": {
          "viewName": "Page2"
        },
        "page3": {
          "viewName": "Page3"
        },
        "page4": {
          "viewName": "Page4"
        }
      }
    }
  }
}
