<mvc:View
	controllerName="test.sap.ui.comp.smartfield.SmartFieldTypes.controller.NoSmartContainer"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:smartField="sap.ui.comp.smartfield"
	xmlns:smartForm="sap.ui.comp.smartform">

	<Page
		id="detail"
		title="Analytical parameters page"
		showNavButton="true"
		navButtonPress=".onNavBack"
		class="sapUiResponsiveContentPadding">
		<VBox id="myVboxLabel" />
		<VBox id="myVbox" />
		<Button text="Set SmartField to visible" press=".setVisible"/>
		<Button text="Clone" press=".doClone"/>
		<List id="List">
			<CustomListItem id="ListItem">
				<smartField:SmartLabel labelFor="ProductId"/>
				<smartField:SmartField id="ProductId" value="{ProductId}" />
			</CustomListItem>
		</List>
	</Page>
</mvc:View>
