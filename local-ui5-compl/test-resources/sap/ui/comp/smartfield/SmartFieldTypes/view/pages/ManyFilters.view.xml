<mvc:View controllerName="test.sap.ui.comp.smartfield.SmartFieldTypes.controller.ManyFilters"
	xmlns="sap.m"
	xmlns:core="sap.ui.core"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:smartField="sap.ui.comp.smartfield"
	xmlns:smartForm="sap.ui.comp.smartform">
	<Page
		id="detail"
		title="ValueHelpDialog with many filters page"
		showNavButton="true"
		navButtonPress=".onNavBack"
		class="sapUiResponsiveContentPadding">
		<smartForm:SmartForm id="form" validationMode="Async" editTogglable="true" editable="true">
			<smartForm:layout>
				<smartForm:ColumnLayout id="FormLayout" columnsXL="4" columnsL="3" columnsM="2"/>
			</smartForm:layout>
			<smartForm:Group title="ValueHelp dialog with many filters">
				<smartForm:GroupElement>
					<smartField:SmartField id="VH_many_filters" value="{VH_many_filters}">
						<smartField:customData>
							<core:CustomData key="defaultFilterBarExpanded" value='true'/>
							<core:CustomData key="defaultShowAllFilters" value='true'/>
						</smartField:customData>
					</smartField:SmartField>
				</smartForm:GroupElement>
				<smartForm:GroupElement>
					<smartField:SmartField id="VH_visible_hidden_filters" value="{VH_visible_hidden_filters}">
						<smartField:customData>
							<core:CustomData key="defaultFilterBarExpanded" value='true'/>
						</smartField:customData>
					</smartField:SmartField>
				</smartForm:GroupElement>
			</smartForm:Group>
		</smartForm:SmartForm>
	</Page>
</mvc:View>
