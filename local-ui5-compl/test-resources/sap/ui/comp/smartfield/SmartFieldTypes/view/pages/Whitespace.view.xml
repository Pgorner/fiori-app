<mvc:View
	controllerName="test.sap.ui.comp.smartfield.SmartFieldTypes.controller.Whitespace"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:smartField="sap.ui.comp.smartfield"
	xmlns:smartForm="sap.ui.comp.smartform">

	<Page id="detail"
		title="Whitespaces page"
		showNavButton="true"
		navButtonPress=".onNavBack"
		class="sapUiResponsiveContentPadding">
		<smartForm:SmartForm id="form" validationMode="Async" editTogglable="true"  editable="true">
			<smartForm:layout>
				<smartForm:ColumnLayout id="FormLayout" columnsXL="4" columnsL="3" columnsM="2"/>
			</smartForm:layout>
			<smartForm:Group title="Whitespace" id="WhiteSpace">
				<smartForm:GroupElement>
					<smartField:SmartField id="StringWs" value="{StringWS}" contextEditable="true" width="14rem"/>
				</smartForm:GroupElement>
				<smartForm:GroupElement>
					<smartField:SmartField id="StringFixedWs" value="{StringFixedWS}" contextEditable="true" width="14rem"/>
				</smartForm:GroupElement>
				<smartForm:GroupElement>
					<smartField:SmartField id="SuggestionsWS" value="{SuggestionsWS}" contextEditable="true" width="14rem"/>
				</smartForm:GroupElement>
			</smartForm:Group>
		</smartForm:SmartForm>
	</Page>
</mvc:View>
