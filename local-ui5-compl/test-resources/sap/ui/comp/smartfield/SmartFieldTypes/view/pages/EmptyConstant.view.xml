<mvc:View
	controllerName="test.sap.ui.comp.smartfield.SmartFieldTypes.controller.EmptyConstant"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:smartField="sap.ui.comp.smartfield"
	xmlns:smartForm="sap.ui.comp.smartform">

	<Page
		id="detail"
		title="ValueList with constant parameters page"
		showNavButton="true"
		navButtonPress=".onNavBack"
		class="sapUiResponsiveContentPadding">
		<smartForm:SmartForm id="form" validationMode="Async" editTogglable="true"  editable="true">
			<smartForm:layout>
				<smartForm:ColumnLayout id="FormLayout" columnsXL="4" columnsL="3" columnsM="2"/>
			</smartForm:layout>
			<smartForm:Group title="ValueList with Empty constant parameters">
				<smartForm:GroupElement>
					<smartField:SmartField id="constant1" value="{Constant1}" textInEditModeSource="ValueList"/>
				</smartForm:GroupElement>
				<smartForm:GroupElement>
					<smartField:SmartField id="constant2" value="{Constant2}" textInEditModeSource="ValueList"/>
				</smartForm:GroupElement>
				<smartForm:GroupElement>
					<smartField:SmartField id="constant3" value="{Constant3}" textInEditModeSource="ValueList"/>
				</smartForm:GroupElement>
				<smartForm:GroupElement>
					<smartField:SmartField id="constant4" value="{Constant4}" textInEditModeSource="ValueList"/>
				</smartForm:GroupElement>
			</smartForm:Group>
		</smartForm:SmartForm>
	</Page>
</mvc:View>
