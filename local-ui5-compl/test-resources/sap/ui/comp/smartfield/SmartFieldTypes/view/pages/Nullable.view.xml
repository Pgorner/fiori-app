<mvc:View
	controllerName="test.sap.ui.comp.smartfield.SmartFieldTypes.controller.Nullable"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:smartField="sap.ui.comp.smartfield"
	xmlns:smartForm="sap.ui.comp.smartform">
	<Page
		id="detail"
		title="Mandatory fields and nullable annotation page"
		showNavButton="true"
		navButtonPress=".onNavBack"
		class="sapUiResponsiveContentPadding">
		<smartForm:SmartForm id="form" validationMode="Async" editTogglable="true" editable="true">
			<smartForm:layout>
				<smartForm:ColumnLayout id="FormLayout" columnsXL="4" columnsL="3" columnsM="2"/>
			</smartForm:layout>
			<smartForm:Group title="Numeric fields with nullable=false">
				<smartForm:GroupElement>
					<smartField:SmartField id="IINT16" value="{IINT16}"/>
				</smartForm:GroupElement>
				<smartForm:GroupElement>
					<smartField:SmartField id="IDECIMAL" value="{IDECIMAL}"/>
				</smartForm:GroupElement>
				<smartForm:GroupElement>
					<smartField:SmartField id="IDOUBLE" value="{IDOUBLE}"/>
				</smartForm:GroupElement>
				<smartForm:GroupElement>
					<smartField:SmartField id="ISINGLE" value="{ISINGLE}"/>
				</smartForm:GroupElement>
				<smartForm:GroupElement>
					<smartField:SmartField id="IINT32" value="{IINT32}"/>
				</smartForm:GroupElement>
				<smartForm:GroupElement>
					<smartField:SmartField id="IINT64" value="{IINT64}"/>
				</smartForm:GroupElement>
				<smartForm:GroupElement>
					<smartField:SmartField id="IBYTE" value="{IBYTE}"/>
				</smartForm:GroupElement>
				<smartForm:GroupElement>
					<smartField:SmartField id="ISBYTE" value="{ISBYTE}"/>
				</smartForm:GroupElement>
			</smartForm:Group>

			<smartForm:Group title="Numeric fields with nullable=false mandatory">
				<smartForm:GroupElement>
					<smartField:SmartField id="MINT16" value="{MINT16}" clientSideMandatoryCheck="false"/>
				</smartForm:GroupElement>
				<smartForm:GroupElement>
					<smartField:SmartField id="MDECIMAL" value="{MDECIMAL}" clientSideMandatoryCheck="false"/>
				</smartForm:GroupElement>
				<smartForm:GroupElement>
					<smartField:SmartField id="MDOUBLE" value="{MDOUBLE}" clientSideMandatoryCheck="false"/>
				</smartForm:GroupElement>
				<smartForm:GroupElement>
					<smartField:SmartField id="MSINGLE" value="{MSINGLE}" clientSideMandatoryCheck="false"/>
				</smartForm:GroupElement>
				<smartForm:GroupElement>
					<smartField:SmartField id="MINT32" value="{MINT32}" clientSideMandatoryCheck="false"/>
				</smartForm:GroupElement>
				<smartForm:GroupElement>
					<smartField:SmartField id="MINT64" value="{MINT64}" clientSideMandatoryCheck="false"/>
				</smartForm:GroupElement>
				<smartForm:GroupElement>
					<smartField:SmartField id="MBYTE" value="{MBYTE}" clientSideMandatoryCheck="false"/>
				</smartForm:GroupElement>
				<smartForm:GroupElement>
					<smartField:SmartField id="MSBYTE" value="{MSBYTE}" clientSideMandatoryCheck="false"/>
				</smartForm:GroupElement>
			</smartForm:Group>
		</smartForm:SmartForm>
	</Page>
</mvc:View>
