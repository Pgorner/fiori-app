<mvc:View
	controllerName="test.sap.ui.comp.smartfield.SmartFieldTypes.controller.CalendarTypes"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:smartField="sap.ui.comp.smartfield"
	xmlns:smartForm="sap.ui.comp.smartform">

	<Page
		id="detail"
		title="Calendar types page"
		showNavButton="true"
		navButtonPress=".onNavBack"
		class="sapUiResponsiveContentPadding">
		<smartForm:SmartForm id="form" validationMode="Async" editTogglable="true" editable="{view>/editMode}">
			<smartForm:layout>
				<smartForm:ColumnLayout id="FormLayout" columnsXL="4" columnsL="3" columnsM="2"/>
			</smartForm:layout>
			<smartForm:Group>
			<smartForm:GroupElement>
					<smartField:SmartField id="CalendarDate" value="{CalendarDate}" contextEditable="true" width="14rem"/>
				</smartForm:GroupElement>

				<smartForm:GroupElement>
					<smartField:SmartField id="CalendarYear" value="{CalendarYear}" contextEditable="true" width="14rem"/>
				</smartForm:GroupElement>

				<smartForm:GroupElement>
					<smartField:SmartField id="CalendarWeek" value="{CalendarWeek}" contextEditable="true" width="14rem"/>
				</smartForm:GroupElement>

				<smartForm:GroupElement>
					<smartField:SmartField id="CalendarMonth" value="{CalendarMonth}" contextEditable="true" width="14rem"/>
				</smartForm:GroupElement>

				<smartForm:GroupElement>
					<smartField:SmartField id="CalendarQuarter" value="{CalendarQuarter}" contextEditable="true" width="14rem"/>
				</smartForm:GroupElement>

				<smartForm:GroupElement>
					<smartField:SmartField id="CalendarYearWeek" value="{CalendarYearWeek}" contextEditable="true" width="14rem"/>
				</smartForm:GroupElement>

				<smartForm:GroupElement>
					<smartField:SmartField id="CalendarYearMonth" value="{CalendarYearMonth}" contextEditable="true" width="14rem"/>
				</smartForm:GroupElement>

				<smartForm:GroupElement>
					<smartField:SmartField id="CalendarYearQuarter" value="{CalendarYearQuarter}" contextEditable="true" width="14rem"/>
				</smartForm:GroupElement>
			</smartForm:Group>
		</smartForm:SmartForm>
	</Page>
</mvc:View>
