<mvc:View
	controllerName="test.sap.ui.comp.smartfield.SmartFieldTypes.controller.Importance"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:smartField="sap.ui.comp.smartfield"
	xmlns:smartForm="sap.ui.comp.smartform">
	<Page
		id="detail"
		title="Importance Page"
		showNavButton="true"
		navButtonPress=".onNavBack"
		class="sapUiResponsiveContentPadding">
		<smartForm:SmartForm id="form" validationMode="Async" editTogglable="true" editable="{view>/editMode}">
			<smartForm:layout>
				<smartForm:ColumnLayout id="FormLayout" columnsXL="4" columnsL="3" columnsM="2"/>
			</smartForm:layout>
			<smartForm:Group title="Importance" id="Importance">
				<smartForm:GroupElement>
					<smartField:SmartField id="HighImportance" value="{HighImportance}" importance="High"/>
				</smartForm:GroupElement>
				<smartForm:GroupElement>
					<smartField:SmartField id="MediumImportance" value="{MediumImportance}" importance="Medium"/>
				</smartForm:GroupElement>
				<smartForm:GroupElement>
					<smartField:SmartField id="LowImportance" value="{LowImportance}" />
				</smartForm:GroupElement>
			</smartForm:Group>
		</smartForm:SmartForm>
	</Page>
</mvc:View>
