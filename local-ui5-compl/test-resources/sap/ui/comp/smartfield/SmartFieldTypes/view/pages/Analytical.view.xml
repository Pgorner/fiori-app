<mvc:View
	controllerName="test.sap.ui.comp.smartfield.SmartFieldTypes.controller.Analytical"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:smartField="sap.ui.comp.smartfield"
	xmlns:smartForm="sap.ui.comp.smartform">

	<Page
		id="detail"
		title="Analytical parameters page"
		showNavButton="true"
		navButtonPress=".onNavBack"
		class="sapUiResponsiveContentPadding">
		<smartForm:SmartForm id="form" validationMode="Async" editTogglable="true"  editable="true">
			<smartForm:layout>
				<smartForm:ColumnLayout id="FormLayout" columnsXL="4" columnsL="3" columnsM="2"/>
			</smartForm:layout>
			<smartForm:Group title="ValueHelp dialog with analytical parameters">
				<smartForm:GroupElement>
					<smartField:SmartField id="languageCode22" value="{LanguageCode22}" textInEditModeSource="ValueList"/>
				</smartForm:GroupElement>
			</smartForm:Group>
		</smartForm:SmartForm>
		<HBox>
			<Button id="clearRequestsLog" text="Clear Requests LOG" press=".clearRequestsLog" class="sapUiSmallMarginEnd"/>
		</HBox>
		<List id="requests" headerText="Requests LOG:"/>
	</Page>
</mvc:View>
