<mvc:View
	controllerName="test.sap.ui.comp.smartfield.SmartFieldTypes.controller.SortOrder"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:smartField="sap.ui.comp.smartfield"
	xmlns:smartForm="sap.ui.comp.smartform">
	<Page id="detail"
			title="PresentationVariant annotation page"
			showNavButton="true"
			navButtonPress=".onNavBack"
			class="sapUiResponsiveContentPadding">
		<smartForm:SmartForm id="form" validationMode="Async" editTogglable="true"  editable="true">
			<smartForm:layout>
				<smartForm:ColumnLayout id="FormLayout" columnsXL="4" columnsL="3" columnsM="2"/>
			</smartForm:layout>
		<smartForm:Group title="PresentationVariant SortOrder" id="PresentationVariantSortOrder">
				<smartForm:GroupElement>
					<smartField:SmartField id="PresentationVariantSortOrderField" value="{PresentationVariantSortOrder}" contextEditable="true" width="14rem" change="updateCodeEditors"/>
				</smartForm:GroupElement>
			</smartForm:Group>
		</smartForm:SmartForm>
	</Page>
</mvc:View>
