<mvc:View
	controllerName="test.sap.ui.comp.smartfield.SmartFieldTypes.controller.PageList"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc">
	<Page
		id="allPagesListPage"
		title="Page with list of all pages"
		showNavButton="true"
		navButtonPress=".onNavBack"
		class="sapUiResponsiveContentPadding">
		<content>
			<List id="pagesList" items="{path: '/listItems'}">
				<items>
					<StandardListItem
						title="{Name}"
						iconDensityAware="false"
						iconInset="false"
						type="Navigation"
						press=".onListItemPressed" />
				</items>
			</List>
		</content>
	</Page>
</mvc:View>