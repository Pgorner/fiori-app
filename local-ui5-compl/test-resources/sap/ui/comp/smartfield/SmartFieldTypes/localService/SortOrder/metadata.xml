<?xml version="1.0" encoding="utf-8"?>
<edmx:Edmx Version="1.0"
		   xmlns:edmx="http://schemas.microsoft.com/ado/2007/06/edmx"
		   xmlns:m="http://schemas.microsoft.com/ado/2007/08/dataservices/metadata"
		   xmlns:sap="http://www.sap.com/Protocols/SAPData">
	<edmx:DataServices m:DataServiceVersion="2.0">
		<Schema xmlns="http://schemas.microsoft.com/ado/2008/09/edm" Namespace="MyNamespace" sap:schema-version="1">

			<EntityType Name="MyEntityType">
				<Key>
					<PropertyRef Name="ID" />
				</Key>
				<Property Name="ID" Type="Edm.String" Nullable="false" MaxLength="20" sap:label="Product ID" sap:creatable="false" sap:updatable="false" sap:sortable="false"/>
				<Property Name="PresentationVariantSortOrder" Type="Edm.String" MaxLength="3" sap:label="Company Names fixed values ordered by PresentationVariant SortOrder Annotation" sap:updatable="true" sap:value-list="fixed-values"/>
			</EntityType>

			<EntityType Name="SortOrderVHType" sap:content-version="1">
				<Key>
					<PropertyRef Name="Key"/>
				</Key>
				<Property Name="Key" Nullable="false" Type="Edm.String" sap:label="Company Code"/>
				<Property Name="Town" Type="Edm.String" sap:label="Town"/>
				<Property Name="CompanyName" Type="Edm.String" sap:label="Company Name"/>
				<Property Name="Currency" Type="Edm.String" sap:label="Currency"/>
			</EntityType>

			<EntityContainer m:IsDefaultEntityContainer="true"
							 sap:supported-formats="atom json">
				<EntitySet Name="SortOrder" EntityType="MyNamespace.MyEntityType" sap:creatable="true" sap:updatable="true" sap:deletable="false" sap:addressable="false" sap:content-version="1" />
				<EntitySet Name="SortOrderVH" EntityType="MyNamespace.SortOrderVHType" sap:creatable="false" sap:updatable="true" sap:deletable="false" sap:addressable="false" sap:content-version="1" />
			</EntityContainer>

			<Annotations Target="MyNamespace.MyEntityType/PresentationVariantSortOrder"
		xmlns="http://docs.oasis-open.org/odata/ns/edm">
				<Annotation Term="com.sap.vocabularies.Common.v1.ValueList">
					<Record>
						<PropertyValue Property="Label" String="PresentationVariantSortOrder"/>
						<PropertyValue Property="CollectionPath" String="SortOrderVH"/>
						<PropertyValue Property="SearchSupported" Bool="false"/>
						<PropertyValue Property="Parameters">
							<Collection>
								<Record Type="com.sap.vocabularies.Common.v1.ValueListParameterInOut">
									<PropertyValue Property="LocalDataProperty" PropertyPath="PresentationVariantSortOrder"/>
									<PropertyValue Property="ValueListProperty" String="CompanyName"/>
								</Record>
							</Collection>
						</PropertyValue>
					</Record>
				</Annotation>
			</Annotations>

			<Annotations Target="MyNamespace.SortOrderVHType"
				xmlns="http://docs.oasis-open.org/odata/ns/edm">
					<Annotation Term="com.sap.vocabularies.UI.v1.PresentationVariant">
						<Record>
							<PropertyValue Property="SortOrder">
								<Collection>
									<Record Type="com.sap.vocabularies.Common.v1.SortOrderType">
										<PropertyValue Property="Property" PropertyPath="Key"/>
										<PropertyValue Property="Descending" Bool="true"/>
									</Record>
								</Collection>
							</PropertyValue>
						</Record>
				</Annotation>
			</Annotations>
		</Schema>
	</edmx:DataServices>
</edmx:Edmx>