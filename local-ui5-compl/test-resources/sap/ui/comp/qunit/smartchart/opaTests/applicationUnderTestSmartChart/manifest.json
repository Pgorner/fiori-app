{
	"_version": "1.8.0",
	"sap.app": {
		"id": "applicationUnderTestSmartChart",
		"type": "application",
		"title": "SAPUI5 Smart Chart Example",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"ach": "CA-UI5-CMP",
		"dataSources": {
			"main": {
				"uri": "/my/mock/data",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5"
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "applicationUnderTestSmartChart.view.Main",
			"type": "XML",
			"async": true
		},
		"dependencies": {
			"minUI5Version": "1.54",
			"libs": {
				"sap.m": {},
				"sap.ui.comp": {}
			}
		},
		"models": {
			"": {
				"dataSource": "main",
				"settings": {
					"disableHeadRequestForToken": true
				}
			}
		}
	}
}