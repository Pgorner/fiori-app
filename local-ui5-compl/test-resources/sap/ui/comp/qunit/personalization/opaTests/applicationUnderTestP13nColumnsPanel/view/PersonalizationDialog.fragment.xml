<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:p13n="sap.m.p13n">
	<p13n:Popup showReset="true" showResetEnabled="{path: '/ShowResetEnabled'}" ok="onOK" cancel="onCancel" reset="onReset" class="sapUiSizeCompact">
		<p13n:panels>
			<P13nColumnsPanel changeColumnsItems="onChangeColumnsItems"
							  items="{path: '/Items'}"
							  columnsItems="{path: '/ColumnsItems'}">
				<items>
					<P13nItem columnKey="{columnKey}" text="{text}"/>
				</items>
				<columnsItems>
					<P13nColumnsItem columnKey="{columnKey}" index="{index}" visible="{visible}"/>
				</columnsItems>
			</P13nColumnsPanel>

		</p13n:panels>
	</p13n:Popup>
</core:FragmentDefinition>
