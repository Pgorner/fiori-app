<?xml version="1.0" encoding="utf-8"?>
<edmx:Edmx Version="1.0"
	xmlns:edmx="http://schemas.microsoft.com/ado/2007/06/edmx"
	xmlns:m="http://schemas.microsoft.com/ado/2007/08/dataservices/metadata"
	xmlns:sap="http://www.sap.com/Protocols/SAPData">
	<edmx:Reference xmlns:edmx="http://docs.oasis-open.org/odata/ns/edmx"
		Uri="https://sap.github.io/odata-vocabularies/vocabularies/Common/v1">
		<edmx:Include Alias="Common" Namespace="com.sap.vocabularies.Common.v1"/>
	</edmx:Reference>
	<edmx:Reference xmlns:edmx="http://docs.oasis-open.org/odata/ns/edmx"
		Uri="https://sap.github.io/odata-vocabularies/vocabularies/UI/v1">
		<edmx:Include Alias="UI" Namespace="com.sap.vocabularies.UI.v1"/>
	</edmx:Reference>
		<edmx:Reference xmlns:edmx="http://docs.oasis-open.org/odata/ns/edmx"
		Uri="https://sap.github.io/odata-vocabularies/vocabularies/CodeList/v1">
		<edmx:Include Alias="CodeList" Namespace="com.sap.vocabularies.CodeList.v1"/>
	</edmx:Reference>
	<edmx:DataServices m:DataServiceVersion="2.0">
		<Schema Namespace="Namespace" sap:schema-version="1"
			xmlns="http://schemas.microsoft.com/ado/2008/09/edm">
			<EntityType Name="MainEntityType" sap:content-version="1">
				<Key>
					<PropertyRef Name="ID"/>
				</Key>
				<Property Name="ID"
					Type="Edm.String"
					Nullable="false"
					sap:label="Product ID"/>
				<Property Name="Name"
					Type="Edm.String"
					sap:label="Company Name"/>
				<Property Name="Currency"
					Type="Edm.String"
					sap:label="Currency"/>
				<Property Name="Customer"
					Type="Edm.String"
					sap:label="Customer"/>
				<Property Name="String_HiddenField"
					Type="Edm.String"
					sap:label="Field annotated with Hidden annotation"/>
				<Property Name="VH_many_filters"
					Type="Edm.String"
					sap:label="Value HelpDialog with many filters"/>
			</EntityType>
			<EntityType Name="VL1Type" sap:content-version="1">
				<Key>
					<PropertyRef Name="Key"/>
				</Key>
				<Property Name="Key"
					Nullable="false"
					Type="Edm.String"
					sap:label="Company Code"/>
				<Property Name="Town"
					Type="Edm.String"
					sap:label="Town"/>
				<Property Name="CompanyName"
					Type="Edm.String"
					sap:label="Company Name"/>
			</EntityType>
			<EntityType Name="VL2Type" sap:content-version="1">
				<Key>
					<PropertyRef Name="CurrencyCode" />
				</Key>
				<Property Name="CurrencyCode"
					Type="Edm.String"
					Nullable="false"
					MaxLength="4"
					sap:display-format="UpperCase"
					sap:label="Currency Code" />
				<Property Name="CurrencyText"
					Type="Edm.String"
					sap:label="Description" />
				<Property Name="DeprecationCode"
					Type="Edm.String"
					MaxLength="1"
					sap:label="Company Deprecation Code" />
			</EntityType>
			<EntityType Name="VL3Type" sap:content-version="1">
				<Key>
					<PropertyRef Name="VH_many_filters" />
				</Key>
				<Property Name="VH_many_filters"
					Type="Edm.String"
					sap:label="VH Filter1" />
				<Property Name="VH_many_filters2"
					Type="Edm.String"
					sap:label="VH Filter2" />
				<Property Name="VH_many_filters3"
					Type="Edm.String"
					sap:label="VH Filter3" />
				<Property Name="VH_many_filters4"
					Type="Edm.String"
					sap:label="VH Filter4" />
				<Property Name="VH_many_filters5"
					Type="Edm.String"
					sap:label="VH Filter5" />
				<Property Name="VH_many_filters6"
					Type="Edm.String"
					sap:label="VH Filter6" />
				<Property Name="VH_many_filters7"
					Type="Edm.String"
					sap:label="VH Filter7" />
				<Property Name="VH_many_filters8"
					Type="Edm.String"
					sap:label="VH Filter8" />
				<Property Name="VH_many_filters9"
					Type="Edm.String"
					sap:label="VH Filter9" />
				<Property Name="VH_many_filters10"
					Type="Edm.String"
					sap:label="VH Filter10" />
			</EntityType>

			<EntityContainer Name="Namespace_Entities" m:IsDefaultEntityContainer="true" sap:supported-formats="atom json xlsx">
				<EntitySet Name="MainEntitySet" EntityType="Namespace.MainEntityType" sap:content-version="1"/>
				<EntitySet Name="VL1" EntityType="Namespace.VL1Type" sap:content-version="1"/>
				<EntitySet Name="VL2" EntityType="Namespace.VL2Type" sap:content-version="1"/>
				<EntitySet Name="VL3" EntityType="Namespace.VL3Type" sap:content-version="1"/>
			</EntityContainer>

			<Annotations Target="Namespace.MainEntityType/Name"
				xmlns="http://docs.oasis-open.org/odata/ns/edm">
				<Annotation Term="Common.ValueListWithFixedValues" Bool="true"/>
				<Annotation Term="Common.ValueList">
					<Record>
						<PropertyValue Property="CollectionPath" String="VL1"/>
						<PropertyValue Property="SearchSupported" Bool="true"/>
						<PropertyValue Property="Parameters">
							<Collection>
								<Record Type="Common.ValueListParameterInOut">
									<PropertyValue Property="LocalDataProperty" PropertyPath="Name"/>
									<PropertyValue Property="ValueListProperty" String="CompanyName"/>
								</Record>
							</Collection>
						</PropertyValue>
					</Record>
				</Annotation>
			</Annotations>
			<Annotations xmlns="http://docs.oasis-open.org/odata/ns/edm" Target="Namespace.MainEntityType/Currency">
				<Annotation Term="Common.ValueList">
					<Record>
						<PropertyValue Property="Label" String="Currencies" />
						<PropertyValue Property="CollectionPath" String="VL2" />
						<PropertyValue Property="SearchSupported" Bool="true" />
						<PropertyValue Property="Parameters">
							<Collection>
								<Record Type="com.sap.vocabularies.Common.v1.ValueListParameterInOut">
									<PropertyValue Property="LocalDataProperty" PropertyPath="Currency" />
									<PropertyValue Property="ValueListProperty" String="CurrencyCode" />
								</Record>
								<Record Type="com.sap.vocabularies.Common.v1.ValueListParameterDisplayOnly">
									<PropertyValue Property="ValueListProperty" String="CurrencyText" />
								</Record>
								<Record
									Type="Common.ValueListParameterDisplayOnly">
									<PropertyValue Property="ValueListProperty"
										String="DeprecationCode" />
								</Record>
							</Collection>
						</PropertyValue>
					</Record>
				</Annotation>
			</Annotations>
			<Annotations xmlns="http://docs.oasis-open.org/odata/ns/edm" Target="Namespace.MainEntityType/VH_many_filters">
				<Annotation Term="Common.ValueList">
					<Record>
						<PropertyValue Property="Label" String="ValueHelpDialog with many filters" />
						<PropertyValue Property="CollectionPath" String="VL3" />
						<PropertyValue Property="SearchSupported" Bool="true" />
						<PropertyValue Property="Parameters">
							<Collection>
								<Record Type="com.sap.vocabularies.Common.v1.ValueListParameterInOut">
									<PropertyValue Property="LocalDataProperty" PropertyPath="VH_many_filters" />
									<PropertyValue Property="ValueListProperty" String="VH_many_filters" />
								</Record>
							</Collection>
						</PropertyValue>
					</Record>
				</Annotation>
			</Annotations>

			<!-- The values from the value list for Key property are visualised in descending order. -->
			<Annotations Target="Namespace.VL1Type" xmlns="http://docs.oasis-open.org/odata/ns/edm">
				<Annotation Term="UI.PresentationVariant">
					<Record>
						<PropertyValue Property="SortOrder">
							<Collection>
								<Record Type="Common.SortOrderType">
									<PropertyValue Property="Property" PropertyPath="Key"/>
									<PropertyValue Property="Descending" Bool="true"/>
								</Record>
							</Collection>
						</PropertyValue>
					</Record>
				</Annotation>
			</Annotations>

			<!-- DeprecationCode is visualising the status (valid, deprecated or revoked) as a letter in the ValueHelpDialog. -->
			<Annotations xmlns="http://docs.oasis-open.org/odata/ns/edm" Target="Namespace.VL2Type/DeprecationCode">
				<Annotation Term="CodeList.IsConfigurationDeprecationCode" Bool="true"/>
			</Annotations>

			<!-- String_HiddenField is  not rendered in an application, not used in the SmartFilterBar and also not used as filter. -->
			<Annotations xmlns="http://docs.oasis-open.org/odata/ns/edm" Target="Namespace.MainEntityType/String_HiddenField">
				<Annotation Term="UI.Hidden"/>
			</Annotations>

			<!-- Customer is not rendered in the SmartFilterBar control, but still used as filter. -->
			<Annotations  xmlns="http://docs.oasis-open.org/odata/ns/edm" Target="Namespace.MainEntityType/Customer">
				<Annotation Term="UI.HiddenFilter"/>
			</Annotations>

			<Annotations Target="Namespace.MainEntityType/Customer" xmlns="http://docs.oasis-open.org/odata/ns/edm">
				<Annotation Term="Common.FilterDefaultValue" String="Maria Anders" />
			</Annotations>

			<Annotations Target="Namespace.MainEntityType"
				xmlns="http://docs.oasis-open.org/odata/ns/edm">

				<Annotation Term="UI.LineItem">
					<Collection>
						<Record Type="UI.DataField">
							<PropertyValue Property="Value" Path="Name" />
						</Record>
						<Record Type="UI.DataField">
							<PropertyValue Property="Value" Path="Customer" />
						</Record>
						<Record Type="UI.DataField">
							<PropertyValue Property="Value" Path="Currency" />
						</Record>
					</Collection>
				</Annotation>
			</Annotations>

		</Schema>
	</edmx:DataServices>
</edmx:Edmx>
