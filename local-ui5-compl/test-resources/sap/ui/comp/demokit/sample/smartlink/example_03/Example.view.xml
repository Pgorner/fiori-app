<mvc:View
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.ui.comp.smarttable"
	xmlns:l="sap.ui.layout"
	controllerName="sap.ui.comp.sample.smartlink.example_03.Example">

	<l:VerticalLayout class="sapUiContentPadding" width="100%">
		<l:content>
			<SmartTable tableType="Table" enableAutoBinding="true" entitySet="ProductCollection" useVariantManagement="false"
						useTablePersonalisation="false" showRowCount="false" enableExport="false" enableAutoColumnWidth="true"/>
		</l:content>
	</l:VerticalLayout>

</mvc:View>
