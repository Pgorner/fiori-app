<edmx:Edmx xmlns:edmx="http://schemas.microsoft.com/ado/2007/06/edmx"
		   xmlns:m="http://schemas.microsoft.com/ado/2007/08/dataservices/metadata"
		   xmlns:sap="http://www.sap.com/Protocols/SAPData" Version="1.0">

	<edmx:Reference xmlns:edmx="http://docs.oasis-open.org/odata/ns/edmx"
					Uri="https://sap.github.io/odata-vocabularies/vocabularies/Common/v1">
		<edmx:Include Alias="Common" Namespace="com.sap.vocabularies.Common.v1"/>
	</edmx:Reference>

	<edmx:DataServices m:DataServiceVersion="2.0">
		<Schema xmlns="http://schemas.microsoft.com/ado/2008/09/edm" Namespace="SAP" xml:lang="en" sap:schema-version="1">

			<EntityType Name="MainType" sap:content-version="1">
				<Key>
					<PropertyRef Name="ID" />
				</Key>
				<Property Name="ID" Type="Edm.String" Nullable="false" sap:creatable="false" sap:updatable="false" sap:sortable="false"/>
				<Property Name="PRODUCT_ID" Type="Edm.String" sap:label="Product"/>
				<Property Name="USER_TYPE" Type="Edm.String" sap:label="User Type" sap:value-list="fixed-values"/>
				<Property Name="IS_MANAGER" Type="Edm.Boolean" sap:label="Is Manager"/>
			</EntityType>

			<EntityType Name="ProductType" sap:content-version="1">
				<Key>
					<PropertyRef Name="ID" />
				</Key>
				<Property Name="ID" Type="Edm.String"/>
				<Property Name="COLOR" Type="Edm.String" sap:label="Color"/>
				<Property Name="NAME" Type="Edm.String" sap:label="Name"/>
				<Property Name="PRICE" Type="Edm.String" sap:label="Price"/>
				<Property Name="WEIGHT" Type="Edm.String" sap:label="Weight"/>
				<Property Name="WIDTH" Type="Edm.String" sap:label="Width"/>
				<Property Name="HEIGHT" Type="Edm.String" sap:label="Height"/>
			</EntityType>

			<EntityType Name="UserType" sap:content-version="1">
				<Key>
					<PropertyRef Name="ID" />
				</Key>
				<Property Name="ID" Type="Edm.String"/>
				<Property Name="TYPE" Type="Edm.String" sap:label="User Type"/>
			</EntityType>

			<EntityContainer Name="SAP_Entities" m:IsDefaultEntityContainer="true" sap:supported-formats="atom json xlsx">
				<EntitySet Name="Main" EntityType="SAP.MainType" sap:creatable="false" sap:updatable="true" sap:deletable="false" sap:addressable="false" sap:content-version="1" />
				<EntitySet Name="Product" EntityType="SAP.ProductType" sap:creatable="false" sap:updatable="true" sap:deletable="false" sap:addressable="false" sap:content-version="1" />
				<EntitySet Name="User" EntityType="SAP.UserType" sap:creatable="false" sap:updatable="true" sap:deletable="false" sap:addressable="false" sap:content-version="1" />
			</EntityContainer>

			<Annotations Target="SAP.MainType/PRODUCT_ID" xmlns="http://docs.oasis-open.org/odata/ns/edm">
				<Annotation Term="Common.ValueListRelevantQualifiers">
					<Collection>
						<String/> <!--  Show the value help without qualifier always -->
						<String>ShowAlways</String> <!--  Show the value help with the qualifier 'ShowAlways' always -->
						<If>
							<Eq>
								<Path>USER_TYPE</Path> <!--Show the value help with the qualifier 'Personnel' if field USER_TYPE == personnel -->
								<String>personnel</String>
							</Eq>
							<String>Personnel</String>
						</If>
						<If>
							<Path>IS_MANAGER</Path> <!--Show the value help with the qualifier 'Manager' if the boolean field IS_MANAGER is true -->
							<String>Manager</String>
						</If>
						<If>
							<Eq>
								<Path>USER_TYPE</Path> <!--Show the value help with the qualifier 'Customer' if field USER_TYPE == customer -->
								<String>customer</String>
							</Eq>
							<String>Customer</String>
						</If>
					</Collection>
				</Annotation>
				<Annotation Term="Common.ValueList" >
					<Record>
						<PropertyValue Property="Label" String="Products Defaults"/>
						<PropertyValue Property="CollectionPath" String="Product"/>
						<PropertyValue Property="SearchSupported" Bool="true"/>
						<PropertyValue Property="Parameters">
							<Collection>
								<Record Type="Common.ValueListParameterInOut">
									<PropertyValue Property="LocalDataProperty" PropertyPath="PRODUCT_ID"/>
									<PropertyValue Property="ValueListProperty" String="ID"/>
								</Record>
								<Record Type="Common.ValueListParameterDisplayOnly">
									<PropertyValue Property="ValueListProperty" String="NAME"/>
								</Record>
							</Collection>
						</PropertyValue>
					</Record>
				</Annotation>
				<Annotation Term="Common.ValueList" Qualifier="ShowAlways">
					<Record>
						<PropertyValue Property="Label" String="Products Details that should always be shown"/>
						<PropertyValue Property="CollectionPath" String="Product"/>
						<PropertyValue Property="SearchSupported" Bool="true"/>
						<PropertyValue Property="Parameters">
							<Collection>
								<Record Type="Common.ValueListParameterInOut">
									<PropertyValue Property="LocalDataProperty" PropertyPath="PRODUCT_ID"/>
									<PropertyValue Property="ValueListProperty" String="ID"/>
								</Record>
								<Record Type="Common.ValueListParameterDisplayOnly">
									<PropertyValue Property="ValueListProperty" String="WEIGHT"/>
								</Record>
								<Record Type="Common.ValueListParameterDisplayOnly">
									<PropertyValue Property="ValueListProperty" String="WIDTH"/>
								</Record>
								<Record Type="Common.ValueListParameterDisplayOnly">
									<PropertyValue Property="ValueListProperty" String="HEIGHT"/>
								</Record>
							</Collection>
						</PropertyValue>
					</Record>
				</Annotation>
				<Annotation Term="Common.ValueList" Qualifier="Personnel">
					<Record>
						<PropertyValue Property="Label" String="Products Detailed Specifications for Personnel"/>
						<PropertyValue Property="CollectionPath" String="Product"/>
						<PropertyValue Property="SearchSupported" Bool="true"/>
						<PropertyValue Property="Parameters">
							<Collection>
								<Record Type="Common.ValueListParameterInOut">
									<PropertyValue Property="LocalDataProperty" PropertyPath="PRODUCT_ID"/>
									<PropertyValue Property="ValueListProperty" String="ID"/>
								</Record>
								<Record Type="Common.ValueListParameterDisplayOnly">
									<PropertyValue Property="ValueListProperty" String="NAME"/>
								</Record>
								<Record Type="Common.ValueListParameterDisplayOnly">
									<PropertyValue Property="ValueListProperty" String="COLOR"/>
								</Record>
								<Record Type="Common.ValueListParameterDisplayOnly">
									<PropertyValue Property="ValueListProperty" String="WEIGHT"/>
								</Record>
								<Record Type="Common.ValueListParameterDisplayOnly">
									<PropertyValue Property="ValueListProperty" String="WIDTH"/>
								</Record>
								<Record Type="Common.ValueListParameterDisplayOnly">
									<PropertyValue Property="ValueListProperty" String="HEIGHT"/>
								</Record>
							</Collection>
						</PropertyValue>
					</Record>
				</Annotation>
				<Annotation Term="Common.ValueList" Qualifier="Manager">
					<Record>
						<PropertyValue Property="Label" String="Products Detailed Specifications for Managers"/>
						<PropertyValue Property="CollectionPath" String="Product"/>
						<PropertyValue Property="SearchSupported" Bool="true"/>
						<PropertyValue Property="Parameters">
							<Collection>
								<Record Type="Common.ValueListParameterInOut">
									<PropertyValue Property="LocalDataProperty" PropertyPath="PRODUCT_ID"/>
									<PropertyValue Property="ValueListProperty" String="ID"/>
								</Record>
								<Record Type="Common.ValueListParameterDisplayOnly">
									<PropertyValue Property="ValueListProperty" String="NAME"/>
								</Record>
								<Record Type="Common.ValueListParameterDisplayOnly">
									<PropertyValue Property="ValueListProperty" String="PRICE"/>
								</Record>
							</Collection>
						</PropertyValue>
					</Record>
				</Annotation>
				<Annotation Term="Common.ValueList" Qualifier="Customer">
					<Record>
						<PropertyValue Property="Label" String="Products Specifications for Customers"/>
						<PropertyValue Property="CollectionPath" String="Product"/>
						<PropertyValue Property="SearchSupported" Bool="true"/>
						<PropertyValue Property="Parameters">
							<Collection>
								<Record Type="Common.ValueListParameterInOut">
									<PropertyValue Property="LocalDataProperty" PropertyPath="PRODUCT_ID"/>
									<PropertyValue Property="ValueListProperty" String="ID"/>
								</Record>
								<Record Type="Common.ValueListParameterDisplayOnly">
									<PropertyValue Property="ValueListProperty" String="NAME"/>
								</Record>
								<Record Type="Common.ValueListParameterDisplayOnly">
									<PropertyValue Property="ValueListProperty" String="COLOR"/>
								</Record>
							</Collection>
						</PropertyValue>
					</Record>
				</Annotation>
			</Annotations>

			<Annotations Target="SAP.MainType/USER_TYPE" xmlns="http://docs.oasis-open.org/odata/ns/edm">
				<Annotation Term="Common.ValueList">
					<Record>
						<PropertyValue Property="Label" String="User Types"/>
						<PropertyValue Property="CollectionPath" String="User"/>
						<PropertyValue Property="SearchSupported" Bool="true"/>
						<PropertyValue Property="Parameters">
							<Collection>
								<Record Type="Common.ValueListParameterInOut">
									<PropertyValue Property="LocalDataProperty" PropertyPath="USER_TYPE"/>
									<PropertyValue Property="ValueListProperty" String="TYPE"/>
								</Record>
							</Collection>
						</PropertyValue>
					</Record>
				</Annotation>
			</Annotations>
		</Schema>
	</edmx:DataServices>
</edmx:Edmx>
