<?xml version="1.0" encoding="utf-8"?>
<edmx:Edmx Version="1.0"
	xmlns:edmx="http://schemas.microsoft.com/ado/2007/06/edmx"
	xmlns:m="http://schemas.microsoft.com/ado/2007/08/dataservices/metadata"
	xmlns:sap="http://www.sap.com/Protocols/SAPData">
	<edmx:Reference xmlns:edmx="http://docs.oasis-open.org/odata/ns/edmx"
					Uri="https://sap.github.io/odata-vocabularies/vocabularies/Common/v1">
		<edmx:Include Alias="Common" Namespace="com.sap.vocabularies.Common.v1"/>
	</edmx:Reference>
	<edmx:Reference xmlns:edmx="http://docs.oasis-open.org/odata/ns/edmx"
					Uri="https://sap.github.io/odata-vocabularies/vocabularies/UI/v1">
		<edmx:Include Alias="UI" Namespace="com.sap.vocabularies.UI.v1"/>
	</edmx:Reference>
	<edmx:DataServices m:DataServiceVersion="2.0">
		<Schema Namespace="smartfield.PresentationVariantSortOrderAnnotation" sap:schema-version="1"
			xmlns="http://schemas.microsoft.com/ado/2008/09/edm">
			<EntityType Name="Product" sap:content-version="1">
				<Key>
					<PropertyRef Name="ID"/>
				</Key>
				<Property Name="ID" Type="Edm.String" Nullable="false" MaxLength="20" sap:label="Product ID" sap:creatable="false" sap:updatable="true" sap:sortable="false"/>
				<Property Name="Name" Type="Edm.String" sap:label="Product Company" sap:creatable="false" sap:updatable="true" sap:sortable="false"/>
			</EntityType>
			<EntityType Name="VL_CompaniesType" sap:content-version="1">
				<Key>
					<PropertyRef Name="Key"/>
				</Key>
				<Property Name="Key" Nullable="false" Type="Edm.String" sap:label="Company Code"/>
				<Property Name="Town" Type="Edm.String" sap:label="Town"/>
				<Property Name="CompanyName" Type="Edm.String" sap:label="Company Name"/>
				<Property Name="Currency" Type="Edm.String" sap:label="Currency"/>
			</EntityType>

			<EntityContainer Name="smartfield.PresentationVariantSortOrderAnnotation_Entities" m:IsDefaultEntityContainer="true" sap:supported-formats="atom json xlsx">
				<EntitySet Name="Products" EntityType="smartfield.PresentationVariantSortOrderAnnotation.Product" sap:creatable="false" sap:updatable="true" sap:deletable="false" sap:pageable="false" sap:content-version="1"/>
				<EntitySet Name="VL_Companies" EntityType="smartfield.PresentationVariantSortOrderAnnotation.VL_CompaniesType" sap:creatable="false" sap:updatable="false" sap:deletable="false" sap:content-version="1" sap:countable="false"/>
			</EntityContainer>

			<Annotations Target="smartfield.PresentationVariantSortOrderAnnotation.VL_Companies/CompanyName"
				xmlns="http://docs.oasis-open.org/odata/ns/edm">
				<Annotation Term="Common.Text" Path="Town">
					<Annotation Term="UI.TextArrangement" EnumMember="UI.TextArrangementType/TextLast"/>
				</Annotation>
			</Annotations>

			<Annotations Target="smartfield.PresentationVariantSortOrderAnnotation.VL_CompaniesType"
				xmlns="http://docs.oasis-open.org/odata/ns/edm">
				<Annotation Term="UI.PresentationVariant">
					<Record>
						<PropertyValue Property="SortOrder">
							<Collection>
								<Record Type="Common.SortOrderType">
									<PropertyValue Property="Property" PropertyPath="Key"/>
									<PropertyValue Property="Descending" Bool="true"/>
								</Record>
							</Collection>
						</PropertyValue>
					</Record>
				</Annotation>
			</Annotations>

			<Annotations Target="smartfield.PresentationVariantSortOrderAnnotation.Product/Name"
				xmlns="http://docs.oasis-open.org/odata/ns/edm">
				<Annotation Term="Common.ValueListWithFixedValues" Bool="true"/>
				<Annotation Term="Common.ValueList">
					<Record>
						<PropertyValue Property="Label" String="Categories"/>
						<PropertyValue Property="CollectionPath" String="VL_Companies"/>
						<PropertyValue Property="SearchSupported" Bool="true"/>
						<PropertyValue Property="Parameters">
							<Collection>
								<Record Type="Common.ValueListParameterInOut">
									<PropertyValue Property="LocalDataProperty" PropertyPath="Name"/>
									<PropertyValue Property="ValueListProperty" String="CompanyName"/>
								</Record>
							</Collection>
						</PropertyValue>
					</Record>
				</Annotation>
			</Annotations>
		</Schema>
	</edmx:DataServices>
</edmx:Edmx>
