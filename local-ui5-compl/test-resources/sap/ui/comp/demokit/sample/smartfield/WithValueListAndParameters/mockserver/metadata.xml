<edmx:Edmx xmlns:edmx="http://schemas.microsoft.com/ado/2007/06/edmx"
		   xmlns:m="http://schemas.microsoft.com/ado/2007/08/dataservices/metadata"
		   xmlns:sap="http://www.sap.com/Protocols/SAPData" Version="1.0">
	<edmx:Reference xmlns:edmx="http://docs.oasis-open.org/odata/ns/edmx"
					Uri="https://sap.github.io/odata-vocabularies/vocabularies/Common/v1">
		<edmx:Include Alias="Common" Namespace="com.sap.vocabularies.Common.v1"/>
	</edmx:Reference>
	<edmx:DataServices m:DataServiceVersion="2.0">
		<Schema xmlns="http://schemas.microsoft.com/ado/2008/09/edm" Namespace="MyNamespace" xml:lang="en" sap:schema-version="1">

			<EntityType Name="MainType" sap:content-version="1">
				<Key>
					<PropertyRef Name="ID" />
				</Key>
				<Property Name="ID" Type="Edm.String" Nullable="false" sap:creatable="false" sap:updatable="false" sap:sortable="false"/>
				<Property Name="CODE" Type="Edm.String" sap:label="Language Code without any constants"/>
				<Property Name="CODE_ASIA" Type="Edm.String" sap:label="Language Code filtered with Asia as a constant"/>
				<Property Name="CODE_EUROPE" Type="Edm.String" sap:label="Language Code filtered with Europe as a constant"/>
				<Property Name="InKey" Type="Edm.String" sap:label="City (In Field)"/>
				<Property Name="InKeyFixedValues" Type="Edm.String" sap:label="City (In Field)"/>
				<Property Name="InKeySelect" Type="Edm.String" sap:label="City (In Field)"/>
				<Property Name="InOutKey" Type="Edm.String" sap:label="Company Name (Input InOut)"/>
				<Property Name="InOutKeyFixedValues" Type="Edm.String" sap:label="Company Name (ComboBox InOut)" sap:value-list="fixed-values"/>
				<Property Name="InOutKeySelect" Type="Edm.String"  sap:label="Company Name (Select InOut)"/>
				<Property Name="OutKey" Type="Edm.String"  sap:label="Currency (Out Field)"/>
				<Property Name="OutKeyFixedValues" Type="Edm.String" sap:label="Currency (Out Field)"/>
				<Property Name="OutKeySelect" Type="Edm.String" sap:label="Currency (Out Field)"/>
			</EntityType>

			<EntityType Name="LanguageType" sap:content-version="1">
				<Key>
					<PropertyRef Name="ID" />
				</Key>
				<Property Name="ID" Type="Edm.String"/>
				<Property Name="CODE" Type="Edm.String" sap:label="Language Code"/>
				<Property Name="NAME" Type="Edm.String" sap:label="Language Name"/>
				<Property Name="CONTINENT" Type="Edm.String" sap:label="Continent"/>
			</EntityType>

			<EntityType Name="MYInOutResult" sap:content-version="1">
				<Key>
					<PropertyRef Name="Key" />
				</Key>
				<Property Name="Key" Type="Edm.String" sap:label="Company Code"/>
				<Property Name="InKey" Type="Edm.String" sap:label="City"/>
				<Property Name="Text" Type="Edm.String" sap:label="Company Name"/>
				<Property Name="Currency" Type="Edm.String" sap:label="Currency"/>
			</EntityType>

			<EntityContainer Name="SAP_Entities" m:IsDefaultEntityContainer="true" sap:supported-formats="atom json xlsx">
				<EntitySet Name="Main" EntityType="MyNamespace.MainType" sap:creatable="false" sap:updatable="true" sap:deletable="false" sap:addressable="false" sap:content-version="1" />
				<EntitySet Name="Language" EntityType="MyNamespace.LanguageType" sap:creatable="false" sap:updatable="true" sap:deletable="false" sap:addressable="false" sap:content-version="1" />
				<EntitySet Name="InOuts" EntityType="MyNamespace.MYInOutResult" sap:creatable="false" sap:updatable="true" sap:deletable="false" sap:addressable="false" sap:content-version="1" />
			</EntityContainer>

			<Annotations Target="MyNamespace.MainType/CODE"
						 xmlns="http://docs.oasis-open.org/odata/ns/edm">
				<Annotation Term="Common.ValueList">
					<Record>
						<PropertyValue Property="Label" String="Languages"/>
						<PropertyValue Property="CollectionPath" String="Language"/>
						<PropertyValue Property="SearchSupported" Bool="true"/>
						<PropertyValue Property="Parameters">
							<Collection>
								<Record Type="Common.ValueListParameterInOut">
									<PropertyValue Property="LocalDataProperty" PropertyPath="CODE"/>
									<PropertyValue Property="ValueListProperty" String="CODE"/>
								</Record>
								<Record Type="Common.ValueListParameterDisplayOnly">
									<PropertyValue Property="ValueListProperty" String="NAME"/>
								</Record>
								<Record Type="Common.ValueListParameterDisplayOnly">
									<PropertyValue Property="ValueListProperty" String="CONTINENT"/>
								</Record>
							</Collection>
						</PropertyValue>
					</Record>
				</Annotation>
			</Annotations>

			<Annotations Target="MyNamespace.MainType/CODE_ASIA"
						 xmlns="http://docs.oasis-open.org/odata/ns/edm">
				<Annotation Term="Common.ValueList">
					<Record>
						<PropertyValue Property="Label" String="Languages in Asia"/>
						<PropertyValue Property="CollectionPath" String="Language"/>
						<PropertyValue Property="SearchSupported" Bool="true"/>
						<PropertyValue Property="Parameters">
							<Collection>
								<Record Type="Common.ValueListParameterInOut">
									<PropertyValue Property="LocalDataProperty" PropertyPath="CODE_ASIA"/>
									<PropertyValue Property="ValueListProperty" String="CODE"/>
								</Record>
								<Record Type="Common.ValueListParameterDisplayOnly">
									<PropertyValue Property="ValueListProperty" String="NAME"/>
								</Record>
								<Record Type="Common.ValueListParameterConstant">
									<PropertyValue Property="Constant" String="Asia"/>
									<PropertyValue Property="ValueListProperty" String="CONTINENT"/>
								</Record>
							</Collection>
						</PropertyValue>
					</Record>
				</Annotation>
			</Annotations>

			<Annotations Target="MyNamespace.MainType/CODE_EUROPE"
						 xmlns="http://docs.oasis-open.org/odata/ns/edm">
				<Annotation Term="Common.ValueList">
					<Record>
						<PropertyValue Property="Label" String="Languages in Europe"/>
						<PropertyValue Property="CollectionPath" String="Language"/>
						<PropertyValue Property="SearchSupported" Bool="true"/>
						<PropertyValue Property="Parameters">
							<Collection>
								<Record Type="Common.ValueListParameterInOut">
									<PropertyValue Property="LocalDataProperty" PropertyPath="CODE_EUROPE"/>
									<PropertyValue Property="ValueListProperty" String="CODE"/>
								</Record>
								<Record Type="Common.ValueListParameterDisplayOnly">
									<PropertyValue Property="ValueListProperty" String="NAME"/>
								</Record>
								<Record Type="Common.ValueListParameterConstant">
									<PropertyValue Property="Constant" String="Europe"/>
									<PropertyValue Property="ValueListProperty" String="CONTINENT"/>
								</Record>
							</Collection>
						</PropertyValue>
					</Record>
				</Annotation>
			</Annotations>

			<Annotations Target="MyNamespace.MainType/InOutKey"
						 xmlns="http://docs.oasis-open.org/odata/ns/edm">
				<Annotation Term="Common.ValueList">
					<Record>
						<PropertyValue Property="Label" String="Types"/>
						<PropertyValue Property="CollectionPath" String="InOuts"/>
						<PropertyValue Property="SearchSupported" Bool="true"/>
						<PropertyValue Property="Parameters">
							<Collection>
								<Record Type="Common.ValueListParameterInOut">
									<PropertyValue Property="LocalDataProperty" PropertyPath="InOutKey"/>
									<PropertyValue Property="ValueListProperty" String="Text"/>
								</Record>
								<Record Type="Common.ValueListParameterDisplayOnly">
									<PropertyValue Property="ValueListProperty" String="Key"/>
								</Record>
								<Record Type="Common.ValueListParameterIn">
									<PropertyValue Property="LocalDataProperty" PropertyPath="InKey"/>
									<PropertyValue Property="ValueListProperty" String="InKey"/>
								</Record>
								<Record Type="Common.ValueListParameterOut">
									<PropertyValue Property="LocalDataProperty" PropertyPath="OutKey"/>
									<PropertyValue Property="ValueListProperty" String="Currency"/>
								</Record>
							</Collection>
						</PropertyValue>
					</Record>
				</Annotation>
			</Annotations>

			<Annotations Target="MyNamespace.MainType/InOutKeyFixedValues"
						 xmlns="http://docs.oasis-open.org/odata/ns/edm">
				<Annotation Term="Common.ValueList">
					<Record>
						<PropertyValue Property="Label" String="Types"/>
						<PropertyValue Property="CollectionPath" String="InOuts"/>
						<PropertyValue Property="SearchSupported" Bool="true"/>
						<PropertyValue Property="Parameters">
							<Collection>
								<Record Type="Common.ValueListParameterInOut">
									<PropertyValue Property="LocalDataProperty" PropertyPath="InOutKeyFixedValues"/>
									<PropertyValue Property="ValueListProperty" String="Text"/>
								</Record>
								<Record Type="Common.ValueListParameterDisplayOnly">
									<PropertyValue Property="ValueListProperty" String="Text"/>
								</Record>
								<Record Type="Common.ValueListParameterIn">
									<PropertyValue Property="LocalDataProperty" PropertyPath="InKeyFixedValues"/>
									<PropertyValue Property="ValueListProperty" String="InKey"/>
								</Record>
								<Record Type="Common.ValueListParameterOut">
									<PropertyValue Property="LocalDataProperty" PropertyPath="OutKeyFixedValues"/>
									<PropertyValue Property="ValueListProperty" String="Currency"/>
								</Record>
							</Collection>
						</PropertyValue>
					</Record>
				</Annotation>
			</Annotations>

			<Annotations Target="MyNamespace.MainType/InOutKeySelect"
						 xmlns="http://docs.oasis-open.org/odata/ns/edm">
				<Annotation Term="Common.ValueList">
					<Record>
						<PropertyValue Property="Label" String="Types"/>
						<PropertyValue Property="CollectionPath" String="InOuts"/>
						<PropertyValue Property="SearchSupported" Bool="true"/>
						<PropertyValue Property="Parameters">
							<Collection>
								<Record Type="Common.ValueListParameterInOut">
									<PropertyValue Property="LocalDataProperty" PropertyPath="InOutKeySelect"/>
									<PropertyValue Property="ValueListProperty" String="Text"/>
								</Record>
								<Record Type="Common.ValueListParameterDisplayOnly">
									<PropertyValue Property="ValueListProperty" String="Text"/>
								</Record>
								<Record Type="Common.ValueListParameterIn">
									<PropertyValue Property="LocalDataProperty" PropertyPath="InKeySelect"/>
									<PropertyValue Property="ValueListProperty" String="InKey"/>
								</Record>
								<Record Type="Common.ValueListParameterOut">
									<PropertyValue Property="LocalDataProperty" PropertyPath="OutKeySelect"/>
									<PropertyValue Property="ValueListProperty" String="Currency"/>
								</Record>
							</Collection>
						</PropertyValue>
					</Record>
				</Annotation>
			</Annotations>

		</Schema>
	</edmx:DataServices>
</edmx:Edmx>
