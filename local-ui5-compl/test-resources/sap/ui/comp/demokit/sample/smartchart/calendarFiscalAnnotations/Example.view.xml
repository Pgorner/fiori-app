<mvc:View xmlns:core="sap.ui.core"
		  xmlns:mvc="sap.ui.core.mvc"
		  xmlns="sap.ui.comp.smartchart"
		  xmlns:m="sap.m"
		  controllerName="sap.ui.comp.sample.smartchart.calendarFiscalAnnotations.Example"
		  height="100%">

	<m:VBox fitContainer="true">
		<m:MessageStrip
			text="This example shows some IsCalendar* and IsFiscal* annotations. Please drill down to one of the provided Dimensions."
			showIcon="true">
		</m:MessageStrip>

		<SmartChart id="smartChart"
					header="SmartChart using some Calendar and Fiscal Annotations"
					height="100%"
					entitySet="ProductCollection"
					enableAutoBinding="true"
					useVariantManagement="false"
					useChartPersonalisation="false"
					showFullScreenButton="false"
					selectionMode="Multi"
					useTooltip="false"
					showChartTooltip="false"
					showDrillBreadcrumbs="true"
					showDrillButtons="true"
					showDetailsButton="false"
					showSemanticNavigationButton="false"
					showChartTypeSelectionButton="false"
					activateTimeSeries="false">

		</SmartChart>
	</m:VBox>
</mvc:View>
