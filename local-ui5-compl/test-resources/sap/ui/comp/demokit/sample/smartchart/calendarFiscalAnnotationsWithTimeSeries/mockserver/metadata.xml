<?xml version="1.0" encoding="utf-8"?>
<edmx:Edmx Version="1.0"
		   xmlns:edmx="http://schemas.microsoft.com/ado/2007/06/edmx"
		   xmlns:m="http://schemas.microsoft.com/ado/2007/08/dataservices/metadata"
		   xmlns:sap="http://www.sap.com/Protocols/SAPData">
	<edmx:DataServices m:DataServiceVersion="2.0">
		<Schema Namespace="EPM_DEVELOPER_SCENARIO_SRV"
				sap:schema-version="0" xmlns="http://schemas.microsoft.com/ado/2008/09/edm">
			<EntityType Name="Product"
						sap:service-schema-version="1"
						sap:service-version="1"
						sap:semantics="aggregate"
						sap:content-version="1">
				<Key>
					<PropertyRef Name="ProductId"/>
				</Key>
				<Property Name="ProductId" Type="Edm.String" Nullable="false"
						  MaxLength="10"  sap:label="Product ID"
						  sap:sortable="true" sap:filterable="true"/>
				<Property Name="Name" Type="Edm.String" Nullable="false"
						  MaxLength="255" sap:aggregation-role="dimension" sap:label="Name"
						  sap:sortable="true" sap:filterable="true"/>
				<Property Name="Price" Type="Edm.Decimal" Nullable="false"
						  Precision="23" Scale="4" sap:aggregation-role="measure" sap:label="Price"
						  sap:sortable="true" sap:filterable="true" sap:unit="CurrencyCode"/>

				<!--isCalendar Properties -->
				<Property Name="CalendarYearQuarter" Type="Edm.String" MaxLength="5"
						  sap:aggregation-role="dimension" sap:display-format="NonNegative" sap:semantics="yearquarter"
						  sap:label="Calendar Year Quarter"/>
				<Property Name="CalendarYearMonth" Type="Edm.String" MaxLength="6"
						  sap:aggregation-role="dimension" sap:display-format="NonNegative" sap:semantics="yearmonth"
						  sap:label="Calendar Year Month"/>
				<Property Name="CalendarYearWeek" Type="Edm.String" MaxLength="6"
						  sap:aggregation-role="dimension" sap:display-format="NonNegative" sap:semantics="yearweek"
						  sap:label="Calendar Year Week" />

				<!--isFiscal Properties -->
				<Property Name="FiscalYear" Type="Edm.String" MaxLength="4" sap:aggregation-role="dimension"
						  sap:label="Fiscal Year" sap:semantics="fiscalyear"/>
				<Property Name="FiscalYearPeriod" Type="Edm.String" MaxLength="7" sap:aggregation-role="dimension"
						  sap:semantics="fiscalyearperiod" sap:display-format="NonNegative"
						  sap:label="Fiscal Year Period"/>

			</EntityType>

			<EntityContainer Name="EPM_DEVELOPER_SCENARIO_SRV"
							 m:IsDefaultEntityContainer="true">
				<EntitySet Name="ProductCollection" EntityType="EPM_DEVELOPER_SCENARIO_SRV.Product"
						   sap:creatable="false" sap:updatable="false" sap:deletable="false"
						   sap:pageable="false" sap:content-version="1"/>
			</EntityContainer>

			<Annotations Target="EPM_DEVELOPER_SCENARIO_SRV.Product"
						 xmlns="http://docs.oasis-open.org/odata/ns/edm">
				<Annotation Term="com.sap.vocabularies.UI.v1.Chart">
					<Record>
						<PropertyValue Property="ChartType"
									   EnumMember="com.sap.vocabularies.UI.v1.ChartType/Column"/>
						<PropertyValue Property="Dimensions">
							<Collection>
								<PropertyPath>Name</PropertyPath>
								<PropertyPath>FiscalYear</PropertyPath>
							</Collection>
						</PropertyValue>
						<PropertyValue Property="Measures">
							<Collection>
								<PropertyPath>Price</PropertyPath>
							</Collection>
						</PropertyValue>
					</Record>
				</Annotation>
			</Annotations>

			<Annotations Target="EPM_DEVELOPER_SCENARIO_SRV.Product/YearMonthDay"
						 xmlns="http://docs.oasis-open.org/odata/ns/edm">
				<Annotation Term="com.sap.vocabularies.Common.v1.IsCalendarDate" Bool="true"/>
			</Annotations>

			<!--isCalendar Annotations -->
			<Annotations Target="EPM_DEVELOPER_SCENARIO_SRV.Product/CalendarYearQuarter"
						 xmlns="http://docs.oasis-open.org/odata/ns/edm">
				<Annotation Term="com.sap.vocabularies.Common.v1.IsCalendarYearQuarter" Bool="true"/>
			</Annotations>
			<Annotations Target="EPM_DEVELOPER_SCENARIO_SRV.Product/CalendarYearMonth"
						 xmlns="http://docs.oasis-open.org/odata/ns/edm">
				<Annotation Term="com.sap.vocabularies.Common.v1.IsCalendarYearMonth" Bool="true"/>
			</Annotations>
			<Annotations Target="EPM_DEVELOPER_SCENARIO_SRV.Product/CalendarYearWeek">
				<Annotation Term="com.sap.vocabularies.Common.v1.IsCalendarYearWeek" Bool="true"/>
			</Annotations>

			<!--isFiscal Annotations -->
			<Annotations Target="EPM_DEVELOPER_SCENARIO_SRV.Product/FiscalYear"
						 xmlns="http://docs.oasis-open.org/odata/ns/edm">
				<Annotation Term="com.sap.vocabularies.Common.v1.IsFiscalYear" Bool="true"/>
			</Annotations>
			<Annotations Target="EPM_DEVELOPER_SCENARIO_SRV.Product/FiscalPeriod"
						 xmlns="http://docs.oasis-open.org/odata/ns/edm">
				<Annotation Term="com.sap.vocabularies.Common.v1.IsFiscalPeriod" Bool="true"/>
			</Annotations>
		</Schema>
	</edmx:DataServices>
</edmx:Edmx>
